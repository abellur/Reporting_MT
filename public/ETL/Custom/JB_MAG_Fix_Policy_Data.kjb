    <entry>
      <name>Update Policy/Coverage Expiration Date</name>
      <description/>
      <type>SQL</type>
      <sql>UPDATE FACT_POLICYCOVERAGE FPC
SET fpc.policyexpirationdate_id = 20170401, fpc.coverageexpirationdate_id = 20170401
WHERE  fpc.factpolicycoverage_id IN 
       (SELECT fpc1.factpolicycoverage_id
        FROM   fact_policycoverage fpc1, dim_policy dp
        WHERE  fpc1.policy_id = dp.policy_id
        AND    dp.pol_uniqueid = '3500001^420393467'
        AND    fpc1.month_id >= 20170401);


UPDATE fact_policysnapshot FPC
SET fpc.policyexpirationdate_id = 20170401, fpc.coverageexpirationdate_id = 20170401
WHERE  fpc.factpolicysnapshot_id IN 
       (SELECT fpc1.factpolicysnapshot_id
        FROM   fact_policysnapshot fpc1, dim_policy dp
        WHERE  fpc1.policy_id = dp.policy_id
        AND    dp.pol_uniqueid = '3500001^420393467'
        AND    fpc1.month_id >= 20170401);

UPDATE fact_policy FPC
SET fpc.policyexpirationdate_id = 20170401
WHERE  fpc.factpolicy_id IN 
       (SELECT fpc1.factpolicy_id
        FROM   fact_policy fpc1, dim_policy dp
        WHERE  fpc1.policy_id = dp.policy_id
        AND    dp.pol_uniqueid = '3500001^420393467'
        AND    fpc1.month_id >= 20170401);</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>4SBI_DataWarehouse</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>261</xloc>
      <yloc>128</yloc>
    </entry>
    <entry>
      <name>Update Covered Risk Ext ID</name>
      <description/>
      <type>SQL</type>
      <sql>UPDATE fact_policycoverage fpc
SET    fpc.coveredriskextension_id = 
       NVL((SELECT max(dcre1.coveredriskextension_id)
        FROM   dim_coveredriskextension dcre, dim_coveredriskextension dcre1 
        WHERE  dcre.coveredriskextension_id = fpc.coveredriskextension_id
        AND    dcre.cvrsk_uniqueid = dcre1.cvrsk_uniqueid
        AND    dcre1.rsk_effective_from_date &lt; to_date('4/1/2017','mm/dd/yyyy')
        AND    dcre1.rsk_effective_to_date = to_date('4/1/2017','mm/dd/yyyy')),
       fpc.coveredriskextension_id)
WHERE  fpc.factpolicycoverage_id IN 
       (SELECT fpc1.factpolicycoverage_id
        FROM   fact_policycoverage fpc1, dim_policy dp
        WHERE  fpc1.policy_id = dp.policy_id
        AND    dp.pol_uniqueid = '3500001^420393467'
        AND    fpc1.month_id >= 20170401);


UPDATE fact_policysnapshot fpc
SET    fpc.coveredriskextension_id = 
       NVL((SELECT max(dcre1.coveredriskextension_id)
        FROM   dim_coveredriskextension dcre, dim_coveredriskextension dcre1 
        WHERE  dcre.coveredriskextension_id = fpc.coveredriskextension_id
        AND    dcre.cvrsk_uniqueid = dcre1.cvrsk_uniqueid
        AND    dcre1.rsk_effective_from_date &lt; to_date('4/1/2017','mm/dd/yyyy')
        AND    dcre1.rsk_effective_to_date = to_date('4/1/2017','mm/dd/yyyy')),
       fpc.coveredriskextension_id)
WHERE  fpc.factpolicysnapshot_id IN 
       (SELECT fpc1.factpolicysnapshot_id
