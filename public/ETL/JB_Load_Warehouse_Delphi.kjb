      <type>SQL</type>
      <sql>DECLARE 
CURSOR constraint_cur IS
SELECT table_name,
       constraint_name,
       constraint_type
FROM   user_constraints
WHERE  constraint_type = 'R'
AND    table_name NOT LIKE 'VL_%';

CURSOR table_cur IS
SELECT table_name
FROM   user_tables
WHERE  (table_name LIKE 'FACT_%' 
   OR  table_name LIKE 'DIM_%' 
   OR  table_name LIKE 'STG_%'
   OR  table_name LIKE 'DW_%'
   OR  table_name LIKE 'LKUP_%')
  AND  table_name &lt;&gt; 'DW_REFRESH_DATE';

BEGIN
  
  -- Disable all foreign keys ...
  FOR c1 IN constraint_cur LOOP
      EXECUTE IMMEDIATE ('alter table '|| c1.table_name || ' DISABLE CONSTRAINT '||c1.constraint_name);
  END LOOP;
  
  -- Truncate all STG, DIM, FACT tables.
  
  FOR t IN table_cur LOOP
      EXECUTE IMMEDIATE ('truncate table '|| t.table_name);
  END LOOP;

  -- Delete prior refresh records in DW_REFRESH_DATE
  EXECUTE IMMEDIATE ('DELETE dw_refresh_date WHERE main_job IN (''JB_Load_Warehouse_Delphi'',''JB_Load_Warehouse_Delphi_Temp'')');

EXCEPTION
  WHEN OTHERS THEN
    dbms_output.put_line('Clean out error, sqlerrm = '||SQLERRM);
    RAISE;
end;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
