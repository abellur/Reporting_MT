  <step>
    <name>Get unloaded policy transaction</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>4SBI_Staging</connection>
    <sql>SELECT tl.transaction_log_pk
FROM   transaction_log@oltp tl, 
       policy@oltp p,
       transaction_applied_term@oltp tat, 
       policy_term_history@oltp pth
WHERE  tl.policy_fk = p.policy_pk
AND    tl.transaction_log_pk = tat.transaction_log_fk
AND    tat.term_base_record_fk = pth.policy_term_history_pk
AND    pth.record_mode_code NOT IN ('ENDQUOTE','TEMP')
AND    p.policy_cycle_code = 'POLICY'
AND    tl.transaction_status_code = 'COMPLETE'
AND    tl.transaction_type_code NOT IN ('APP','QUEST','ENDQUOTE','QUOTE')
AND    tl.sys_update_time &lt;= ${sql_loadDate}
AND    INSTR('${chunkNumber}',SUBSTR(to_char(p.policy_pk),${CHUNK_POSITION},1)) > 0
AND    NOT EXISTS (SELECT 1 
                   FROM   stg_policytransaction spt 
                   WHERE  spt.pt_sequence = tl.transaction_log_pk)
AND    NOT EXISTS (SELECT 1 
                   FROM   dw_stg_policytransaction_temp@oltp t 
                   WHERE  t.ptt_sequence = tl.transaction_log_pk)</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
