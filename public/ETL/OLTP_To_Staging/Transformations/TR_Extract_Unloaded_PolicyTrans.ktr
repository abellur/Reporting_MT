  <step>
    <name>Load Transaction Log Data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Delphi_OLTP</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT tl.transaction_log_pk,
       x.policy_no,
       tl.policy_fk,
       x.term_base_record_fk,
       x.term_from_date                                   term_effective_from_date,
       x.term_to_date                                     term_effective_to_date,       
       tl.transaction_type_code,            
       tl.transaction_code,
       tl.transaction_status_code,
       tl.endorsement_quote_id,
       GREATEST(tl.effective_from_date, x.term_from_date) effective_from_date,
       TRUNC(tl.accounting_date)                                 accounting_date,
       tl.endorsement_code,
       tl.waived_fee_b,
       TRUNC(tl.tail_quote_date)                                 tail_quote_date,
       tl.force_rerate_b,
       TRUNC(tl.effective_to_date)                               effective_to_date,
       tl.comments,
       tl.legacy_data_id,
       tl.sys_created_by,
       tl.sys_updated_by,
       TRUNC(tl.sys_create_time)                                 sys_create_time,
       TRUNC(tl.sys_update_time)                                 sys_update_time,
       dw_staging_util.Get_Rated_B(tl.transaction_log_pk)        rated_b,
       'N'                                                       risk_cov_update_b,
       sysdate
FROM   Transaction_Log           tl, 
       (
       SELECT  (select PM_Acct_Date.Chk(pth.transaction_log_fk,  pth.accounting_from_date,
                        pth.closing_trans_log_fk,pth.accounting_to_date,
                        tl1.transaction_log_pk,   tl1.accounting_date) from dual) as  chk,
               tl1.*,
               pth.effective_from_date term_from_date,
               pth.effective_to_date term_to_date,
               pth.term_base_record_fk,
               p.policy_no
       FROM    Policy                    p, 
               Transaction_Applied_Term  tat,
               Policy_Term_History       pth,
               Transaction_Code          tc,
               Transaction_Log           tl1
       WHERE   1=1
       AND     tl1.transaction_log_pk = ?
       AND     tl1.policy_fk = p.policy_pk
       AND     tl1.transaction_log_pk = tat.transaction_log_fk
       AND     tl1.transaction_status_code = 'COMPLETE'
       AND     tl1.transaction_type_code NOT IN ('APP','QUEST','QUOTE','ENDQUOTE')
       AND     tl1.transaction_code = tc.code
       AND     pth.term_base_record_fk = tat.term_base_record_fk 
       AND     pth.record_mode_code NOT IN ('ENDQUOTE','TEMP')
       AND     pth.base_record_b = 'N'                        
       AND     p.policy_cycle_code = 'POLICY'
       ) x
WHERE  1=1
AND    tl.transaction_log_pk = x.transaction_log_pk
AND    (x.chk = 0 OR tl.transaction_code IN ('ENDCOIHOLD','ENDPOLADD') OR
        tl.transaction_type_code = 'CHGCLIENT')
ORDER  BY 1,5
</sql>
