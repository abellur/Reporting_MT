  <step>
    <name>Get Mini Risk Info</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Delphi_OLTP</connection>
    <rowlimit>0</rowlimit>
    <sql>WITH x AS 
       (SELECT t1.cm_risk_pk,
               TRUNC((CASE 
                WHEN nvl(t1.effective_from_date,p.policy_start_date) BETWEEN 
                         to_date('1/1/1950','mm/dd/yyyy') AND 
                         to_date('12/31/2050','mm/dd/yyyy') 
                THEN nvl(t1.effective_from_date,p.policy_start_date) 
                ELSE to_date('1/1/1900','mm/dd/yyyy')
               END)) effective_from_date,
               TRUNC((CASE 
                WHEN nvl(t1.effective_to_date,p.policy_end_date) BETWEEN  
                         to_date('1/1/1950','mm/dd/yyyy') AND 
                         to_date('12/31/2050','mm/dd/yyyy') 
                THEN nvl(t1.effective_to_date,p.policy_end_date)                   
                ELSE to_date('1/1/3000','mm/dd/yyyy')
               END)) effective_to_date
        FROM   cm_risk t1, cm_policy p
        WHERE  t1.cm_policy_fk = p.cm_policy_pk),
     d as
       (SELECT rc.code spec_code,
               max(decode(DW_STAGING_UTIL.get_dw_parameter('RISK_SPEC_DESC_TYPE'),
               'LONG',rc.long_description,
               'SHORT',rc.short_description,
               'VAR1',rc.long_desc_variation1,
               'VAR2',rc.long_desc_variation2,
               rc.long_description)) spec_desc
          FROM risk_class rc
        GROUP BY rc.code)
select to_char(r.cm_risk_pk) risk_uniqueID,
       DECODE(SIGN(to_number(to_char(?,'yyyymmdd')) - to_number(to_char(r.sys_create_time,'yyyymmdd'))),-1,'A','C') rsk_transactionType ,
       x.effective_from_date rsk_transactionEffectiveDate,
       r.risk_type rsk_type,
       rt.long_description rsk_typeDescription,
       r.entity_fk,
       x.effective_from_date,
       x.effective_to_date,
       r.risk_spec_code risk_specialty_code,
       rating_county risk_county_code,
       (select long_description
          from county_code
         where code = r.rating_county) risk_county_desc,
       r.char1 risk_char1,
       r.char2 risk_char2,
       r.char3 risk_char3,
       r.entity_fk,
       x.effective_from_date risk_trans_accounting_date,
       r.risk_description,
       r.attachment,
       r.inc_limit,
       r.agg_limit,
       r.inc_deduct,
       r.agg_deduct,
       r.local_jurisdiction,
       r.hospital_name,
       r.agg_attachment,
       substr(r.legacy_data_id,1,instr(r.legacy_data_id,'PYN')-1) legacy_data_id,
       nvl(substr(r.legacy_data_id,1,instr(r.legacy_data_id,'PYN')-1),'N') havelegacyid,
       r.legacy_data_id original_legacy_data_id,
       to_char(r.cm_risk_pk) original_risk_uniqueid,
       r.cm_risk_pk risk_pk,
       -- Issue 178681
       r.practice_state_code mmrsk_practice_state_code,
       r.legacy_data_id      mmrsk_legacy_data_id,
       r.cancel_reason_code  mmrsk_cancel_reason_code,
       r.premium             mmrsk_premium,
       r.class               mmrsk_class,    
       r.rating_basis        mmrsk_rating_basis,
       r.hospital_number     mmrsk_hospital_number,
       r.ref_number          mmrsk_ref_number,
       r.agg_limit_erosion_level mmrsk_agg_limit_erosion_level,
       r.inc_limit_erosion_level mmrsk_inc_limit_erosion_level,
       r.inc_sir             mmrsk_inc_sir,
       r.agg_sir             mmrsk_agg_sir,    
       r.org_hier_fk         mmrsk_org_hier_fk,
       r.org_hier_lvl1_fk    mmrsk_org_hier_lvl1_fk,
       r.org_hier_lvl2_fk    mmrsk_org_hier_lvl2_fk,
       r.org_hier_lvl3_fk    mmrsk_org_hier_lvl3_fk,
       r.org_hier_lvl4_fk    mmrsk_org_hier_lvl4_fk,
       r.org_hier_lvl5_fk    mmrsk_org_hier_lvl5_fk,
       d.spec_desc           mmrsk_spec_desc
from   cm_risk r, 
       cm_pol_risk_type rt,       
       x, 
       d
where  r.sys_update_time > ?
  AND  r.risk_type = rt.cm_pol_risk_Type_code(+)
  AND  r.cm_risk_pk = x.cm_risk_pk
  AND  r.risk_spec_code = d.spec_code(+)
  AND  to_number(SUBSTR(to_char(r.cm_policy_fk),${CHUNK_POSITION},1)) IN ( ${chunkNumber} )
order  by r.sys_update_time</sql>
