    <connection>Delphi_OLTP</connection>
    <sql>SELECT t.sk_recordnum + ${MAX_STGFINANCIALTRANS_ID} STGFINANCIALTRANS_ID,
       '3.0FT'        V_FILE_VERSION,
       'OASIS'        V_SOURCE_SYSTEM,
       SYSDATE        EXTRACT_DATE,
