       t.aging_date,
       t.invoice_date,
       t.fm_trans_type_code,
       t.transaction_status,
       t.transaction_reference_no,
       t.amount,
       b.transactionbasis_uniqueid,
       t.original_trans_amount,
       NVL2(p.poltrans_unique_id,p.poltrans_unique_id,cl.claimtrans_unique_id) original_transaction_uniqueID,
       NVL2(p.poltrans_unique_id,p.ptt_typecode,cl.claim_transaction_code) original_transaction_code,
       t.sub_account_code,
       t.company_uniqueid,
       t.original_transaction_source,
       t.producer_uniqueid,
       t.reason_code,
       t.reason_desc,
       t.transaction_source,
       t.transaction_description,
	   to_char(t.source_record_fk) source_record_fk,
       t.source_table_name,
       -- #165442
       t.record_status
FROM   dw_tmp_sft_transdetail        t,
       dw_tmp_sft_lkup_claimkey      c,
       dw_tmp_sft_lkup_poltrans      p,
       dw_tmp_sft_lkup_clmtrans      cl,
       dw_tmp_sft_lkup_basisuniqueid b
WHERE  t.sk_recordnum = c.sk_recordnum(+)
AND    t.sk_recordnum = p.sk_recordnum(+)
AND    t.sk_recordnum = cl.sk_recordnum(+)
AND    t.sk_recordnum = b.sk_recordnum(+)</sql>
