  <step>
    <name>Get Bill Relation Change</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Delphi_OLTP</connection>
    <rowlimit>0</rowlimit>
    <sql>
-- Billing relation change records
SELECT dw_staging_util.Get_Trans_Acct_New(ptt.ptt_sequence) ptt_accountingdate,
       ptt.pol_uniqueid,
       trunc(p.inception_date) inception_date,
       p.policy_no,
       trunc(p.sys_create_time) policy_create_time,
       pth.effective_from_date,
       pth.effective_to_date,
       pth.issue_state_code,
       pth.issue_company_entity_fk,
       p.policy_type_code,
       (SELECT pt.Long_Description
          FROM policy_type pt
         WHERE pt.code = p.policy_type_code) policy_type_desc,
       pth.pol_rel_stat_type_code pol_curr_status_code,
       pth.sys_created_by policy_created_by,
       pth.sys_updated_by policy_updated_by,
       trunc(pth.sys_update_time) policy_update_time,
       pth.policy_term_code,
       pth.renewal_indicator_code,
       lc.short_description renewal_indicator_desc,
       pth.non_renewal_reason_code,
       prc.short_description non_renewal_reason_desc,
       pth.pol_rel_status_code,
       pth.pol_rel_reason_code,
       pth.renewal_cycle_code,
       pth.process_location_code,
       pth.short_term_b,
       trunc(pth.rolling_ibnr_date) rolling_ibnr_date,
       p.char1 pol_char1,
       p.char2 pol_char2,
       p.char3 pol_char3,
       p.num1 pol_num1,
       p.num2 pol_num2,
       p.num3 pol_num3,
       trunc(p.date1) pol_date1,
       trunc(p.date2) pol_date2,
       trunc(p.date3) pol_date3,
       e.client_id comp_uniqueid,
       dw_staging_util.get_agent_clientid(p.policy_pk,ptt.ptt_accountingdate, ptt.ptt_effectivedate) prdr_uniqueid, --This will be replaced by function
       Dw_Staging_Util.Get_Uw_Clientid(
         p.policy_pk,
         ptt.ptt_sequence,
         pth.effective_from_date,
         pth.effective_to_date,
         ptt.ptt_accountingdate,
         ptt.ptt_effectivedate) uw_uniqueid,
       ba.account_no account_no, 
       ba.long_description account_desc, 
       e1.client_id insured_uniqueid,
       trunc(bs.accounting_from_date) change_eff_date,
       p.policy_cycle_code,
       pth.organization_type_code,
       (SELECT lc1.short_description
          FROM lookup_code lc1
         WHERE lc1.code = pth.organization_type_code
           AND lc1.lookup_type_code = 'ORGANIZATION_TYPE_CODE') organization_type_desc,
       pth.payment_plan_lst,
       p.RECEIVE_DATE,
       p.REFERRAL_CODE,
       p.FACULTATIVE_B,
       p.PROGRAM_CODE,
       p.FORMATTED_POL_NO,
       p.QUESTIONNAIRE_DATE,
       p.PRT_B,
       p.OFF_NUM,
       p.WIP_NUM,
       p.POLICY_LAYER_CODE,
       p.POLICY_FORM_CODE,
       pth.GUARANTEE_DATE,
       pth.BINDER_B,
       pth.TERM_POLICY_NO,
       pth.DECLINATION_DATE,
       pth.CATEGORY_CODE,
       pth.COLLATERAL_B,
       pth.ROLLING_IBNR_DATE,
       pth.HOSP_TIER ,
       pth.CM_YEAR,
       pth.PEER_GROUPS_CODE ,
       pth.POTENTIAL_CANCEL_1,
       pth.POTENTIAL_CANCEL_2,
       pth.ACCOUNT_NO,
       pth.ACCOUNT_IDENTIFIER,
       pth.PL_AGGREGATE,
       pth.GL_AGGREGATE,
       p.legacy_policy_no,
       p.legacy_issue_company_fk,
       p.process_status_code,
       p.requested_eff_date,
       ptt.ptt_sequence,
       prsc.short_description pol_cancel_type_desc,
       prc1.short_description pol_cancel_reason_desc,
       p.policy_pk,
       pth.accounting_from_date,
       pth.accounting_to_date,
       -- Issue 167723
       pth.policy_term_history_pk    
  FROM policy                     p,
       policy_term_history        pth,
       entity                     e,
       entity_role                er,
       entity                     e1,
       Dw_Stg_Policytransaction_temp ptt,
       fm_billing_setup           bs,
       billing_account            ba,
       Lookup_Code lc,
       Policy_Rel_Reason_Code     prc,
       Policy_Rel_Reason_Code     prc1,
       Policy_Rel_Status_Code     prsc
 WHERE bs.source_record_fk = ptt.ptt_pol_fk
   AND bs.source_subsystem_code = 'PM'
   AND bs.billing_account_fk = ba.billing_account_pk
   AND bs.accounting_to_date > ptt.ptt_accountingdate
   AND bs.effective_to_date > ptt.ptt_effectivedate
   AND ptt.ptt_typecode NOT IN ('QUEST')
   --AND ptt.ptt_typecode NOT IN ('CANCEL', 'REINSTATE', 'QUEST')
   AND ptt.ptt_pol_fk = p.policy_pk
   AND p.policy_pk = pth.policy_fk
   AND pth.term_base_record_fk = ptt.ptt_term_base_fk
   AND pth.base_record_b = 'N'
       --  Policy_Term_History:  Find the version as it looked as
       --  of the particular transaction
   AND Pm_Acct_Date.Chk(pth.transaction_log_fk,   pth.accounting_from_date,
                        pth.closing_trans_log_fk, pth.accounting_to_date,
                        ptt.ptt_sequence,         ptt.ptt_accountingdate) = 0 
   AND e.entity_pk = pth.issue_company_entity_fk
   AND er.role_type_code IN ('POLHOLDER','PROSPECT')
   AND er.source_record_fk = p.policy_pk
   AND nvl(er.accounting_from_date, to_date('01/01/1900', 'mm/dd/yyyy')) &lt;=
       ptt.ptt_accountingdate
   AND nvl(er.accounting_to_date, to_date('01/01/3000', 'mm/dd/yyyy')) >
       ptt.ptt_accountingdate
   AND nvl(er.effective_from_date, to_date('01/01/1900', 'mm/dd/yyyy')) &lt;=
       ptt.ptt_effectivedate
   AND nvl(er.effective_to_date, to_date('01/01/3000', 'mm/dd/yyyy')) >
       ptt.ptt_effectivedate
   AND er.entity_fk = e1.entity_pk
   and lc.lookup_type_code(+) = 'RENEWAL_INDICATOR_CODE'
   AND lc.code(+)          = pth.renewal_indicator_code  
   AND prc.code (+) = pth.non_renewal_reason_code
   AND prc.policy_rel_status_code (+) = 'RENEW'
   AND prc1.code (+) = pth.pol_rel_reason_code
   AND prc1.policy_rel_status_code (+) = pth.pol_rel_status_code
   AND prsc.code (+) = pth.pol_rel_status_code
   AND prsc.pol_rel_status_type_code (+) = pth.pol_rel_stat_type_code
   AND ptt.ptt_typesubcode = 'RELCHG'
   AND bs.sys_update_time > ?
   AND bs.sys_update_time &lt;= ?
</sql>
