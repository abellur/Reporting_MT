    <name>Database join Get Claim Key</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Delphi_OLTP</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT x.claim_key
FROM   (    
        SELECT to_char(k.claim_fk) ||'^'|| k.feature_key AS claim_key,
               row_number() OVER (PARTITION BY k.claim_fk ORDER BY k.feature_key) pick_1
        FROM   dw_claim_feature_key k,
               coverage_insured_xref cix
        WHERE  k.claim_fk = ?
          AND  k.coverage_insured_xref_fk = cix.coverage_insured_xref_pk
          AND  cix.coverage_fk = ?) x
WHERE  pick_1 = 1</sql>
