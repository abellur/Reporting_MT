   AND ac.PRODUCER_AGENT_LICENSE_FK = al.agent_license_pk(+)
   AND al.agent_fk = a.agent_pk(+)
   AND a.entity_fk = el.entity_pk(+)
   AND ac.fm_master_fk = m.fm_master_pk(+)
   AND ac.sys_update_time > ?
   AND ac.sys_update_time &lt;= ?         
   AND instr(?,decode(ac.policy_fk,-1,'1',substr(to_char(nvl(ac.policy_fk, -1)),${CHUNK_POSITION},1))) > 0
order by 1</sql>
