  <step>
    <name>get policy data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Delphi_OLTP</connection>
    <rowlimit>0</rowlimit>
    <sql>WITH x AS 
       (SELECT t1.cm_policy_pk,
               TRUNC((CASE 
                WHEN policy_start_date between to_date('1/1/1950','mm/dd/yyyy') and
                                               to_date('12/31/2050','mm/dd/yyyy') 
                THEN policy_start_date 
                ELSE to_date('1/1/1900','mm/dd/yyyy')
               END)) policy_start_date,
               TRUNC((CASE 
                WHEN policy_end_date between to_date('1/1/1950','mm/dd/yyyy') and
                                             to_date('12/31/2050','mm/dd/yyyy') 
                THEN policy_end_date
                ELSE to_date('1/1/3000','mm/dd/yyyy')   
               END)) policy_end_date
        FROM   cm_policy t1)
SELECT NVL(policy_no,' ') policy_no,
       policy_prefix pol_policyNumberPrefix,
       policy_suffix pol_policyNumberSuffix,
       t.cm_policy_pk,
       x.policy_start_date,
       x.policy_end_date,
       policy_type,
       pt.long_description pol_policy_type_desc,
       entity_company_fk,
       entity_insured_fk,
       entity_producer_fk,
       entity_underwriter_fk,
       state_code,
       t.sys_created_by pol_policy_created_by,
       t.sys_updated_by pol_policy_updated_by,
       trunc(t.sys_update_time) pol_policy_update_time,
       cancel_reason_code pol_policy_cancel_reason_cd,      
       x.policy_start_date pol_changeEffectiveDate,
       x.policy_start_date pol_trans_accounting_date,
       policy_description,
       policy_form,
       trunc(retro_date) retro_date,
       lob_code,
       attachment,
       inc_limit,
       agg_limit,
       inc_deduct,
       agg_deduct,
       bank_acct_no,
       t.limit_cost_type_list,
       rating_basis,
       hospital_name,
       group_number,
       -- Issue 179314 Add cm_policy_pk as part of the policy_uniqueID
       t.policy_no || '^PYN' || SUBSTR(t.policy_start_date,-2) || '^' || t.cm_policy_pk policy_uniqueID,
       t.Legacy_Data_Id, 
       -- Issue 178681
       t.jacket_no                 mmpol_jacket_no,
       t.pct_participation         mmpol_pct_participation,
       t.producer                  mmpol_producer,
       t.premium                   mmpol_premium,
       t.type_of_business          mmpol_type_of_business,
       t.division                  mmpol_division,
       t.deductible_cost_type_list mmpol_ded_cost_type_list,
       t.hospital_number           mmpol_hospital_number,
       t.ref_number                mmpol_ref_number,
       t.policy_commission_rate    mmpol_policy_commission_rate,
       t.productline_productgroup  mmpol_productline_productgrp,
       t.servicing_office          mmpol_servicing_office,
       t.agg_attachment            mmpol_agg_attachment,
       t.inc_sir                   mmpol_inc_sir,
       t.agg_sir                   mmpol_agg_sir,
       t.sir_cost_type_list        mmpol_sir_cost_type_list,
       t.agent_id                  mmpol_agent_id,
       t.agent_name                mmpol_agent_name,
       t.agency_id                 mmpol_agency_id,
       t.agency_name               mmpol_agency_name,
       t.org_hier_fk               mmpol_org_hier_fk
  FROM cm_policy t,
       cm_policy_type pt,
       x
 WHERE t.sys_update_time > ?
   AND x.cm_policy_pk = t.cm_policy_pk
   AND t.policy_type = pt.cm_policy_type(+)
   AND to_number(SUBSTR(to_char(t.cm_policy_pk),${CHUNK_POSITION},1)) IN ( ${chunkNumber} )
ORDER  BY t.cm_policy_pk</sql>
