    <sql>SELECT e.entity_pk, 
       er.entity_role_pk ROLE_OR_CLASS_PK, 
       cast (null as number) address_pk,
       cast (null as number) phone_number_pk,
       'ER' process_flag
FROM   entity_role@oltp er, entity@oltp e
WHERE  er.entity_fk = e.entity_pk
AND    er.role_type_code &lt;&gt; 'MINIRISK'
AND    NOT EXISTS 
       (SELECT 1 
        FROM   stg_legalentity sle  
        WHERE  sle.legalentity_uniqueid = e.client_id
        AND    sle.lenty_type = er.role_type_code)
