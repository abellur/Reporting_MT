SELECT e.entity_pk, 
       ec.entity_class_pk ROLE_OR_CLASS_PK, 
       cast (null as number) address_pk,
       cast (null as number) phone_number_pk,
       'EC' process_flag
FROM   entity_class@oltp ec, entity@oltp e
WHERE  ec.entity_fk = e.entity_pk
AND    NOT EXISTS 
       (SELECT 1 
        FROM   stg_legalentity sle  
        WHERE  sle.legalentity_uniqueid = e.client_id
        AND    sle.lenty_type = ec.entity_class_code)
UNION ALL
SELECT e.entity_pk, 
       e.entity_pk ROLE_OR_CLASS_PK, 
       cast (null as number) address_pk,
       cast (null as number) phone_number_pk,
       'E' process_flag
FROM   entity@oltp e
WHERE  NOT EXISTS 
       (SELECT 1 
        FROM   stg_legalentity sle  
        WHERE  sle.legalentity_uniqueid = e.client_id)
AND    NOT EXISTS 
       (SELECT 1 
        FROM   entity_role@oltp er
        WHERE  er.entity_fk = e.entity_pk)
AND    NOT EXISTS
       (SELECT 1
        FROM   entity_class@oltp ec
        WHERE  ec.entity_fk = e.entity_pk)</sql>
