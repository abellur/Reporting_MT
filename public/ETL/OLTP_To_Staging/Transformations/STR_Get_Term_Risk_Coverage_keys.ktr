    <rowlimit>0</rowlimit>
    <sql>select policy_no||'^'||term_base_record_fk pol_uniqueid
 from policy_term_history@OLTP pth,
      policy@OLTP p
where pth.policy_term_history_pk = ?
and   pth.policy_fk = p.policy_pk
and   ? = 'OASIS PM'
and to_number(substr(p.policy_pk,${CHUNK_POSITION},1)) in(${CHUNK_NUMBER})
union all
-- Issue 179314
select policy_no||'^PYN'||substr(to_char(policy_start_date,'yyyy'),-2) || '^' || cm_policy_pk pol_uniqueid
  from cm_policy@OLTP
where cm_policy_pk = ?
  and ? = 'CLAIM_POLICY'
  and to_number(substr(cm_policy_pk,${CHUNK_POSITION},1)) in(${CHUNK_NUMBER})
union all
-- Issue VPAS-2005
-- For record created for Velocity Policy, use external policy id, so we can 
-- map it to policy data loaded from Velocity when loading the claim data to 
-- fact table. Since policy term id is not passed to claim, we have to use 
-- policy term eff/exp date to find proper Velocity record.
SELECT distinct (vsp.policy_number||'^'||vspt.policyterm_uniqueid) policy_uniqueid
FROM   vl_stg_policy vsp, vl_stg_policyterm vspt, cm_policy@OLTP cp1, cm_policy@OLTP cp
WHERE  vsp.policy_number = cp.policy_no
AND    vsp.policy_uniqueid = cp.external_policy_number_id
AND    vsp.policyversion_uniqueid = vspt.policyversion_uniqueid
AND    vsp.transaction_uniqueid = vspt.transaction_uniqueid
AND    vspt.term_start_date = cp.policy_start_date
AND    vspt.term_end_date = cp.policy_end_date
AND    cp1.external_policy_number_id = cp.external_policy_number_id
AND    cp1.cm_policy_pk = ?
AND    ? = 'VELOCITY'
AND    to_number(substr(cp1.cm_policy_pk,${CHUNK_POSITION},1)) in(${CHUNK_NUMBER})
</sql>
