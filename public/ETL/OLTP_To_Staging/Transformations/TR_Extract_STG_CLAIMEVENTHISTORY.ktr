    <connection>Delphi_OLTP</connection>
    <sql>SELECT 'OASIS' AS SOURCE_SYSTEM,
       TO_CHAR(ce.claim_event_history_pk) AS CLAIMEVENTHISTORY_UNIQUEID,
        COALESCE((SELECT C.CLAIM_NO
        FROM CLAIM C
        WHERE C.CLAIM_PK= ce.claim_fk),
        (select o.occurrence_no
        from occurrence o
        where o.occurrence_pk=ce.claim_fk),
        (select p.policy_no
        from policy p
        where p.policy_pk=ce.claim_fk),
        '-1') AS SOURCE_NUMBER,
       ce.event_code,
       TO_CHAR(ce.claim_event_history_fk) AS  PARENT_CLAIMEVENTHISTORY_UID,
       ce.claim_fk as SOURCE_RECORD_FK,
       ce.source_table_name,
       ce.entity_fk,
       ce.orig_sched_date,
       ce.current_sched_date,
       ce.completed_date,
       ce.notes,
       ec.short_description as EVENT_CODE_SHORT_DESC,
       ec.long_description as EVENT_LONG_SHORT_DESC,
       ec.event_type_code,
       ce.form_code,
       ce.file_name,
       ce.sys_create_time,
       ce.sys_created_by,
       ce.sys_update_time,
       ce.sys_updated_by,
       ce.assignor_entity_fk,
       ce.manual_b
FROM   CLAIM_EVENT_HISTORY ce, 
       event_code          ec
WHERE  ce.event_code = ec.event_code(+)
order by  TO_CHAR(ce.claim_event_history_pk)</sql>
