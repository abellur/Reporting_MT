    <sql>SELECT client_id insured_uniqueid
FROM   (SELECT e.client_id, 
               row_number() OVER (PARTITION BY er.source_record_fk ORDER BY er.effective_from_date DESC) chk
        FROM   entity_role er,
               entity e
        WHERE  er.entity_fk = e.entity_pk
        AND    er.source_record_fk = ?
        AND    er.role_type_code = 'POLHOLDER')
