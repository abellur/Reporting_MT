    <connection>4SBI_Staging</connection>
    <sql>SELECT distinct e.entity_pk,
       e.client_id,
       e.title,
       nvl(e.first_name, e.organization_name) first_name,
       nvl(e.last_name, e.dba_name) last_name,
       e.web_address1,
       e.email_address1,
       NVL(e.SOCIAL_SECURITY_NUMBER,e.FEDERAL_TAX_ID) ssn,
       'AGENT' comp_type,
       'Agent' comp_type_desc,
       e.sys_update_time entity_sys_update_time,
       ec.sys_update_time ec_sys_update_time,
       a.sys_update_time agent_sys_update_time,
       a.situation_code agent_type,
       (SELECT lc.short_description
          FROM lookup_code lc, AGENT
         WHERE lc.code = a.situation_code
           AND lc.lookup_type_code = 'SITUATION_CODE'
           AND ROWNUM = 1) agent_type_desc,
       a.effective_start_date,
       a.effective_end_date,
       ? agent_addr_type,
       ? agent_phone_type,
       ? agent_fax_type,
       ? agent_mobile_type,
       ? county_desc_type,
       ? CURRENT_REFRESH_DATE,
       ? LAST_REFRESH_START_TIME
  FROM entity e, entity_class ec, AGENT a
 WHERE e.entity_pk = ec.entity_fk
   AND e.entity_pk = a.entity_fk
   AND nvl(ec.effective_to_date,to_date('1/1/3000','mm/dd/yyyy')) > SYSDATE
   AND ec.entity_class_code =
       dw_staging_util.get_dw_parameter('AGENT_CODE')</sql>
