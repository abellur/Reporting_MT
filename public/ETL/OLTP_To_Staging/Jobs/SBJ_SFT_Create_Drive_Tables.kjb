   AND nvl(ba.accounting_mode, 'BAL_FWD') = 'OPEN_ITEM'
   AND instr('${CHUNK_NUMBER}',decode(i.policy_fk,-1,'1',substr(to_char(i.policy_fk),${CHUNK_POSITION},1))) > 0;</sql>
