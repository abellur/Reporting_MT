  <step>
    <name>Merge DIM_ENTITY (Insert/Update)</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>4SBI_DataWarehouse</connection>
    <execute_each_row>N</execute_each_row>
    <single_statement>N</single_statement>
    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>merge into dim_entity d
using 
(
	select
		EN_ADA_NUMBER ,
		EN_CHAR1 ,
		EN_CHAR2 ,
		EN_CHAR3 ,
		EN_CHAR4 ,
		EN_CHAR5 ,
		NVL(EN_CLAIMS_FREE_DATE,to_date('1900/01/01','yyyy/mm/dd')) EN_CLAIMS_FREE_DATE,
		EN_CLIENT_ID ,
		EN_COMPONENT ,
		NVL(EN_DATE1,to_date('1900/01/01','yyyy/mm/dd')) EN_DATE1,
		NVL(EN_DATE2,to_date('1900/01/01','yyyy/mm/dd')) EN_DATE2,
		NVL(EN_DATE3,to_date('1900/01/01','yyyy/mm/dd')) EN_DATE3,
		NVL(EN_DATE_OF_BIRTH,to_date('1900/01/01','yyyy/mm/dd')) EN_DATE_OF_BIRTH,
		NVL(EN_DATE_OF_DEATH,to_date('1900/01/01','yyyy/mm/dd')) EN_DATE_OF_DEATH,
		NVL(EN_DATE_OF_MATURITY,to_date('1900/01/01','yyyy/mm/dd')) EN_DATE_OF_MATURITY,
		EN_DBA_NAME ,
		EN_DECEASED_B ,
		EN_DEFAULT_TAX_ID ,
		EN_DISCARDED_B ,
		EN_DYNAMIC_CLAIM_ENTRY_SOURCE ,
		EN_EMAIL_ADDRESS1 ,
		EN_EMAIL_ADDRESS2 ,
		EN_EMAIL_ADDRESS3 ,
		EN_ENTITY_STATUS_CODE ,
		EN_ENTITY_TYPE ,
		NVL(EN_FAKE_ADA_NUMBER,0) EN_FAKE_ADA_NUMBER,
		EN_FEDERAL_TAX_ID ,
		EN_FEDERAL_TAX_ID_VERIFIED_B ,
		EN_FIRST_NAME ,
		EN_FIRST_NAME_UPPER ,
		EN_FORMATTED_NAME ,
		EN_GENDER ,
		EN_HICN ,
		EN_INDUSTRY_DESC ,
		NVL(EN_INSURED_SINCE_DATE,to_date('1900/01/01','yyyy/mm/dd')) EN_INSURED_SINCE_DATE,
		EN_LAST_NAME ,
		EN_LAST_NAME_SOUNDEX ,
		EN_LAST_NAME_UPPER ,
		EN_LEGACY_DATA_ID ,
		EN_LEGAL_NAME ,
		NVL(EN_LOSS_FREE_DATE,to_date('1900/01/01','yyyy/mm/dd')) EN_LOSS_FREE_DATE,
		EN_MARITAL_STATUS ,
		EN_MIDDLE_NAME ,
		EN_MINOR_B ,
		NVL(EN_NUM1,0) EN_NUM1,
		NVL(EN_NUM2,0) EN_NUM2,
		NVL(EN_NUM3,0) EN_NUM3,
		EN_ORGANIZATION_NAME ,
		EN_ORGANIZATION_NAME_SOUNDEX ,
		EN_ORGANIZATION_NAME_UPPER ,
		EN_PREFIX_NAME ,
		EN_REFERENCE_NUMBER ,
		EN_SIC_CODE ,
		EN_SOCIAL_SECURITY_NUMBER ,
		EN_SSN_VERIFIED_B ,
		EN_SUFFIX_NAME ,
		EN_SYS_CREATED_BY ,
		NVL(EN_SYS_CREATE_TIME,to_date('1900/01/01','yyyy/mm/dd')) EN_SYS_CREATE_TIME,
		EN_SYS_UPDATED_BY ,
		NVL(EN_SYS_UPDATE_TIME,to_date('1900/01/01','yyyy/mm/dd')) EN_SYS_UPDATE_TIME,
		NVL(EN_TAX_INFO_EFFECTIVE_DATE,to_date('1900/01/01','yyyy/mm/dd')) EN_TAX_INFO_EFFECTIVE_DATE,
		EN_TITLE ,
		EN_VERY_IMPORTANT_PERSON_B ,
		EN_VERY_LONG_NAME ,
		EN_WEB_ADDRESS1 ,
		EN_WEB_ADDRESS2 ,
		EN_WEB_ADDRESS3 ,
		EN_WEB_USER_B ,
		EN_WEB_USER_ID ,
		LOADDATE,
		EN_ENTITY_PK,
        EN_PROF_DESIGNATION,
        ${v_refresh_id} refresh_id,
        'OASIS' source_system,
        1 record_version,
        to_date('1/1/1900', 'mm/dd/yyyy') valid_from,
        to_date('1/1/3000', 'mm/dd/yyyy') valid_to,
        trim(to_char(EN_ENTITY_PK)) EN_CLIENT_UNIQUEID       
	from stg_entity
	WHERE LOADDATE = ${sql_loadDate}
) s
on (d.EN_ENTITY_PK = s.EN_ENTITY_PK)
when matched then
update set
	d.EN_CLIENT_ID = s.EN_CLIENT_ID,
	d.EN_GENDER = s.EN_GENDER,
	d.EN_ENTITY_TYPE = s.EN_ENTITY_TYPE,
	d.EN_FIRST_NAME = s.EN_FIRST_NAME,
	d.EN_FIRST_NAME_UPPER = s.EN_FIRST_NAME_UPPER,
	d.EN_MIDDLE_NAME = s.EN_MIDDLE_NAME,
	d.EN_LAST_NAME = s.EN_LAST_NAME,
	d.EN_LAST_NAME_UPPER = s.EN_LAST_NAME_UPPER,
	d.EN_LAST_NAME_SOUNDEX = s.EN_LAST_NAME_SOUNDEX,
	d.EN_ORGANIZATION_NAME = s.EN_ORGANIZATION_NAME,
	d.EN_ORGANIZATION_NAME_UPPER = s.EN_ORGANIZATION_NAME_UPPER,
	d.EN_ORGANIZATION_NAME_SOUNDEX = s.EN_ORGANIZATION_NAME_SOUNDEX,
	d.EN_PREFIX_NAME = s.EN_PREFIX_NAME,
	d.EN_SUFFIX_NAME = s.EN_SUFFIX_NAME,
	d.EN_VERY_LONG_NAME = s.EN_VERY_LONG_NAME,
	d.EN_FEDERAL_TAX_ID = s.EN_FEDERAL_TAX_ID,
	d.EN_SOCIAL_SECURITY_NUMBER = s.EN_SOCIAL_SECURITY_NUMBER,
	d.EN_VERY_IMPORTANT_PERSON_B = s.EN_VERY_IMPORTANT_PERSON_B,
	d.EN_TITLE = s.EN_TITLE,
	d.EN_DATE_OF_BIRTH = s.EN_DATE_OF_BIRTH,
	d.EN_EMAIL_ADDRESS1 = s.EN_EMAIL_ADDRESS1,
	d.EN_EMAIL_ADDRESS2 = s.EN_EMAIL_ADDRESS2,
	d.EN_EMAIL_ADDRESS3 = s.EN_EMAIL_ADDRESS3,
	d.EN_SSN_VERIFIED_B = s.EN_SSN_VERIFIED_B,
	d.EN_FEDERAL_TAX_ID_VERIFIED_B = s.EN_FEDERAL_TAX_ID_VERIFIED_B,
	d.EN_DEFAULT_TAX_ID = s.EN_DEFAULT_TAX_ID,
	d.EN_TAX_INFO_EFFECTIVE_DATE = s.EN_TAX_INFO_EFFECTIVE_DATE,
	d.EN_MARITAL_STATUS = s.EN_MARITAL_STATUS,
	d.EN_DECEASED_B = s.EN_DECEASED_B,
	d.EN_MINOR_B = s.EN_MINOR_B,
	d.EN_DATE_OF_DEATH = s.EN_DATE_OF_DEATH,
	d.EN_DATE_OF_MATURITY = s.EN_DATE_OF_MATURITY,
	d.EN_DISCARDED_B = s.EN_DISCARDED_B,
	d.EN_SIC_CODE = s.EN_SIC_CODE,
	d.EN_CHAR1 = s.EN_CHAR1,
	d.EN_CHAR2 = s.EN_CHAR2,
	d.EN_CHAR3 = s.EN_CHAR3,
	d.EN_NUM1 = s.EN_NUM1,
	d.EN_NUM2 = s.EN_NUM2,
	d.EN_NUM3 = s.EN_NUM3,
	d.EN_DATE1 = s.EN_DATE1,
	d.EN_DATE2 = s.EN_DATE2,
	d.EN_DATE3 = s.EN_DATE3,
	d.EN_SYS_CREATED_BY = s.EN_SYS_CREATED_BY,
	d.EN_SYS_UPDATED_BY = s.EN_SYS_UPDATED_BY,
	d.EN_SYS_CREATE_TIME = s.EN_SYS_CREATE_TIME,
	d.EN_SYS_UPDATE_TIME = s.EN_SYS_UPDATE_TIME,
	d.EN_LEGACY_DATA_ID = s.EN_LEGACY_DATA_ID,
	d.EN_LOSS_FREE_DATE = s.EN_LOSS_FREE_DATE,
	d.EN_CLAIMS_FREE_DATE = s.EN_CLAIMS_FREE_DATE,
	d.EN_INSURED_SINCE_DATE = s.EN_INSURED_SINCE_DATE,
	d.EN_DYNAMIC_CLAIM_ENTRY_SOURCE = s.EN_DYNAMIC_CLAIM_ENTRY_SOURCE,
	d.EN_INDUSTRY_DESC = s.EN_INDUSTRY_DESC,
	d.EN_FAKE_ADA_NUMBER = s.EN_FAKE_ADA_NUMBER,
	d.EN_COMPONENT = s.EN_COMPONENT,
	d.EN_ENTITY_STATUS_CODE = s.EN_ENTITY_STATUS_CODE,
	d.EN_ADA_NUMBER = s.EN_ADA_NUMBER,
	d.EN_WEB_ADDRESS1 = s.EN_WEB_ADDRESS1,
	d.EN_WEB_ADDRESS2 = s.EN_WEB_ADDRESS2,
	d.EN_WEB_ADDRESS3 = s.EN_WEB_ADDRESS3,
	d.EN_WEB_USER_B = s.EN_WEB_USER_B,
	d.EN_WEB_USER_ID = s.EN_WEB_USER_ID,
	d.EN_DBA_NAME = s.EN_DBA_NAME,
	d.EN_HICN = s.EN_HICN,
	d.EN_LEGAL_NAME = s.EN_LEGAL_NAME,
	d.EN_CHAR4 = s.EN_CHAR4,
	d.EN_CHAR5 = s.EN_CHAR5,
	d.EN_REFERENCE_NUMBER = s.EN_REFERENCE_NUMBER,
	d.EN_FORMATTED_NAME = s.EN_FORMATTED_NAME,
	d.LOADDATE = s.LOADDATE,
    d.EN_PROF_DESIGNATION = s.EN_PROF_DESIGNATION,
    d.REFRESH_ID = s.refresh_id,
    d.SOURCE_SYSTEM = s.source_system,
    d.VALID_FROMDATE = s.valid_from,
    d.VALID_TODATE = s.valid_to,
    d.RECORD_VERSION = s.record_version,
    d.EN_CLIENT_UNIQUEID = s.EN_CLIENT_UNIQUEID
when not matched then
	insert /*+ append */
	(
		EN_ENTITY_PK,
		EN_CLIENT_ID,
		EN_GENDER,
		EN_ENTITY_TYPE,
		EN_FIRST_NAME,
		EN_FIRST_NAME_UPPER,
		EN_MIDDLE_NAME,
		EN_LAST_NAME,
		EN_LAST_NAME_UPPER,
		EN_LAST_NAME_SOUNDEX,
