		EN_ORGANIZATION_NAME_UPPER,
		EN_ORGANIZATION_NAME_SOUNDEX,
		EN_PREFIX_NAME,
		EN_SUFFIX_NAME,
		EN_VERY_LONG_NAME,
		EN_FEDERAL_TAX_ID,
		EN_SOCIAL_SECURITY_NUMBER,
