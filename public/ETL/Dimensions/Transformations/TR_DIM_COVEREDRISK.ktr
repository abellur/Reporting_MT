       rsk_typedescription,
       rsk_address1,
       rsk_address2,
       rsk_address3,
       source_system,
       'dummy' dummy_field,
       CAST (deleted_indicator AS INTEGER) deleted_indicator
FROM   (SELECT risk_uniqueid,
               rsk_number,
               mmrsk_exposure_basis_code rsk_subnumber,
               rsk_type,
               case rsk_typedescription
                 when 'Unknown' then rsk_type
                 else rsk_typedescription
               end rsk_typedescription,
               rsk_address1,
               rsk_address2,
               rsk_address3,
               source_system,
               rsk_transactiontype,
               case rsk_transactiontype
                 when 'D' then 1.0
                 else 0.0
               end DELETED_INDICATOR,
               row_number() over(PARTITION BY risk_uniqueid 
                                 ORDER BY rsk_transactionaccountingdate DESC, pt_sequence DESC) pick1
        FROM   stg_risk
        WHERE  loaddate = ${sql_loadDate}) 
WHERE  pick1 = 1
ORDER  BY risk_uniqueid</sql>
