    <entry>
      <name>Create Precalc</name>
      <description/>
      <type>SQL</type>
      <sql>create table tmp_clm_mon_sum_clm_precalc
nologging
pctfree 0
tablespace 
${G_STAGING_DB_DATA} as
select 
         claim_uniqueid,
         min(claimtransaction_id) min_claimtransaction_id,
         to_number(to_char(transactionstatusdate,'yyyymm')||'01') month_id,
         sum(decode(ctrans_losspaid,
                    '+',
                    1,
                    '-',
                    -1,
                    0) * amount) loss_pd_amt,
         sum(decode(ctrans_lossreserve,
                    '+',
                    1,
                    '-',
                    -1,
                    0) * amount) loss_rsrv_chng_amt,
         sum(decode(ctrans_alaepaid,
                    '+',
                    1,
                    '-',
                    -1,
                    0) * amount) alc_exp_pd_amt,
         sum(decode(ctrans_alaereserve,
                    '+',
                    1,
                    '-',
                    -1,
