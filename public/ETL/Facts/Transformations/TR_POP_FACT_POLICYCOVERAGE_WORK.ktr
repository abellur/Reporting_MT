    <connection>4SBI_DataWarehouse</connection>
    <sql>--     Transactions effective between the beginning date of period year and period end date, 
--     or transaction accounting date is between the beginning date of period year and period end date
--     but Policy term expiration date is before current period end date.
SELECT FIRSTINSURED_ID,
       SECONDINSURED_ID,
       THIRDINSURED_ID,
       FIRSTOTHERINTEREST_ID,
       SECONDOTHERINTEREST_ID,
       LIMIT_ID,
       DEDUCTIBLE_ID,
       CLASS_ID,
       COVEREDRISKEXTENSION_ID,
       POLICYTRANSACTIONEXTENSION_ID,
       FACT_POLICYTRANSACTION.EFFECTIVEDATE_ID,
       FACT_POLICYTRANSACTION.COVERAGEEFFECTIVEDATE_ID,
       FACT_POLICYTRANSACTION.COVERAGEEXPIRATIONDATE_ID,
       UNDERWRITER_ID,
       PRODUCT_ID,
       COMPANY_ID,
       TERRITORY_ID,
       FACT_POLICYTRANSACTION.TRANSACTIONTYPE_ID,
       PRODUCER_ID,
       COVERAGE_ID,
       dpe.POLICYEXTENSION_ID,
       FACT_POLICYTRANSACTION.POLICYEFFECTIVEDATE_ID,
       FACT_POLICYTRANSACTION.POLICYEXPIRATIONDATE_ID,
       FACT_POLICYTRANSACTION.POLICY_ID,
       dp.POL_UNIQUEID,
       DIM_COVERAGEEXTENSION.COV_UNIQUEID,
       PRISK.COVEREDRISK_ID,
       PRISK.CVRSK_UNIQUEID AS PRSK_UNIQUEID,
       COVEREDRISKLOCATION_ID,
       EARNINGSTYPE,
       TRANS_CODE,
       ACCT_DATE AS ACCOUNTINGDATE,
       COVEFF_DATE AS COVERAGEEFFECTIVEDATE,
       COVEXP_DATE AS COVERAGEEXPIRATIONDATE,
       EFF_DATE AS TRANSACTIONEFFECTIVEDATE,
       VDIM_EARNINGSFROMDATE.ERNFRM_DATE AS EARNFROM_DATE,
       VDIM_EARNINGSTODATE.ERNTO_DATE AS EARNTO_DATE,
       MMCOV_COVERAGE_SEGMENT_CODE,
       x.org_pol_expdate,
       SUM(AMOUNT) AS AMOUNT,
       SUM(COMMISSION_AMOUNT) AS COMMISSION_AMOUNT
FROM   FACT_POLICYTRANSACTION,
       VDIM_ACCOUNTINGDATE,
       VDIM_COVERAGEEFFECTIVEDATE,
       VDIM_COVERAGEEXPIRATIONDATE,
       VDIM_EFFECTIVEDATE,
       DIM_TRANSACTIONTYPE,
       VDIM_EARNINGSFROMDATE,
       VDIM_EARNINGSTODATE,
       VDIM_POLICYEXPIRATIONDATE,
       VDIM_POLICYEFFECTIVEDATE,
       DIM_POLICY dp,
       DIM_COVERAGEEXTENSION,
       DIM_COVEREDRISK PRISK,
       DIM_POLICYEXTENSION DPE,
       (SELECT dpe1.policy_uniqueid, MAX(dpe1.pol_exp_date) org_pol_expdate
              FROM   dim_policyextension dpe1
              GROUP  BY dpe1.policy_uniqueid
             ) x 
WHERE  1=1
AND    FACT_POLICYTRANSACTION.ACCOUNTINGDATE_ID = VDIM_ACCOUNTINGDATE.ACCOUNTINGDATE_ID
AND    FACT_POLICYTRANSACTION.COVERAGEEFFECTIVEDATE_ID = VDIM_COVERAGEEFFECTIVEDATE.COVERAGEEFFECTIVEDATE_ID
AND    FACT_POLICYTRANSACTION.COVERAGEEXPIRATIONDATE_ID = VDIM_COVERAGEEXPIRATIONDATE.COVERAGEEXPIRATIONDATE_ID
AND    FACT_POLICYTRANSACTION.EFFECTIVEDATE_ID = VDIM_EFFECTIVEDATE.EFFECTIVEDATE_ID
AND    FACT_POLICYTRANSACTION.TRANSACTIONTYPE_ID = DIM_TRANSACTIONTYPE.TRANSACTIONTYPE_ID
AND    FACT_POLICYTRANSACTION.EARNFROMDATE_ID = VDIM_EARNINGSFROMDATE.EARNFROMDATE_ID
AND    FACT_POLICYTRANSACTION.EARNTODATE_ID = VDIM_EARNINGSTODATE.EARNTODATE_ID
AND    FACT_POLICYTRANSACTION.POLICYEXPIRATIONDATE_ID = VDIM_POLICYEXPIRATIONDATE.POLICYEXPIRATIONDATE_ID
AND    FACT_POLICYTRANSACTION.POLICYEFFECTIVEDATE_ID = VDIM_POLICYEFFECTIVEDATE.POLICYEFFECTIVEDATE_ID
AND    FACT_POLICYTRANSACTION.POLICY_ID = dp.POLICY_ID
AND    FACT_POLICYTRANSACTION.COVERAGEEXTENSION_ID = DIM_COVERAGEEXTENSION.COVERAGEEXTENSION_ID
AND    FACT_POLICYTRANSACTION.Coveredrisk_Id = PRISK.COVEREDRISK_ID
AND    FACT_POLICYTRANSACTION.POLICYEXTENSION_ID = DPE.POLICYEXTENSION_ID
AND    x.policy_uniqueid = dp.pol_uniqueid
AND    INCLUDE_IN_SUMMARY_B = 'Y' 
AND    COVERAGE_ID &lt;&gt; 0
AND    EFF_DATE &lt;= ${sql_periodEndDate} 
AND    POLEFF_DATE &lt;= ${sql_periodStartDate} 
AND    COVEFF_DATE &lt;= ${sql_periodEndDate}
AND    ACCT_DATE &lt;= ${sql_periodEndDate}
AND    EXISTS
      (SELECT 1
       FROM   Fact_Policytransaction    pt1,
              Dim_Policy                dp1,
              Vdim_Policyeffectivedate  vpe,
              Vdim_Effectivedate        vte,
              Vdim_Accountingdate       vat,
              Vdim_Coverageeffectivedate vce,
              Vdim_Policyexpirationdate vpx
       WHERE  pt1.policyeffectivedate_id = vpe.POLICYEFFECTIVEDATE_ID
       AND    pt1.accountingdate_id = vat.ACCOUNTINGDATE_ID
       AND    pt1.effectivedate_id = vte.EFFECTIVEDATE_ID
       AND    pt1.coverageeffectivedate_id = vce.COVERAGEEFFECTIVEDATE_ID
       AND    pt1.policyexpirationdate_id = vpx.POLICYEXPIRATIONDATE_ID
       AND    pt1.policy_id = dp1.policy_id
       AND    dp1.pol_uniqueid = dp.pol_uniqueid
       AND    vpx.POLEXP_DATE &lt;= ${sql_periodStartDate} 
       AND    vte.EFF_DATE &lt;= ${sql_periodEndDate} 
       AND    vpe.POLEFF_DATE &lt;= ${sql_periodStartDate} 
       AND    vce.COVEFF_DATE &lt;= ${sql_periodEndDate}
       AND  (  -- Transaction effective in this year.
              (vte.EFF_DATE &lt;= ${sql_periodEndDate} AND
               vte.EFF_YEAR = ${periodYear})
            OR -- Transaction processed in this year.
              (vat.acct_date &lt;= ${sql_periodEndDate} AND
               vat.acct_year = ${periodYear})
            )
       )
GROUP  BY
       FIRSTINSURED_ID,
       SECONDINSURED_ID,
       THIRDINSURED_ID,
       FIRSTOTHERINTEREST_ID,
       SECONDOTHERINTEREST_ID,
       LIMIT_ID,
       DEDUCTIBLE_ID,
       CLASS_ID,
       COVEREDRISKEXTENSION_ID,
       POLICYTRANSACTIONEXTENSION_ID,
       FACT_POLICYTRANSACTION.EFFECTIVEDATE_ID,
       FACT_POLICYTRANSACTION.COVERAGEEFFECTIVEDATE_ID,
       FACT_POLICYTRANSACTION.COVERAGEEXPIRATIONDATE_ID,
       UNDERWRITER_ID,
       PRODUCT_ID,
       COMPANY_ID,
       TERRITORY_ID,
       FACT_POLICYTRANSACTION.TRANSACTIONTYPE_ID,
       PRODUCER_ID,
       COVERAGE_ID,
       dpe.POLICYEXTENSION_ID,
       FACT_POLICYTRANSACTION.POLICYEFFECTIVEDATE_ID,
       FACT_POLICYTRANSACTION.POLICYEXPIRATIONDATE_ID,
       FACT_POLICYTRANSACTION.POLICY_ID,
       dp.POL_UNIQUEID,
       DIM_COVERAGEEXTENSION.COV_UNIQUEID,
       PRISK.COVEREDRISK_ID,
       PRISK.CVRSK_UNIQUEID,
       COVEREDRISKLOCATION_ID,
       EARNINGSTYPE,
       TRANS_CODE,
       ACCT_DATE,
       COVEFF_DATE,
       COVEXP_DATE,
       EFF_DATE,
       VDIM_EARNINGSFROMDATE.ERNFRM_DATE,
       VDIM_EARNINGSTODATE.ERNTO_DATE,
       MMCOV_COVERAGE_SEGMENT_CODE,
       x.org_pol_expdate
order by ACCOUNTINGDATE, POLICYTRANSACTIONEXTENSION_ID</sql>
