    <sql>select CLAIMEXTENSION_ID prd_claimextension_id
  from (select *
          from (select '1' sort_key, 
                       CLAIMEXTENSION_ID, 
                       valid_fromdate
                  from dim_claimextension
                 where CLAIM_UNIQUEID = ?
                   and valid_fromdate &lt;= ?
                   and valid_todate > ?
                 order by valid_fromdate desc)
         where rownum &lt; 2
        union
        select *
          from (select '2' sort_key, 
                       CLAIMEXTENSION_ID, 
                       valid_fromdate
                  from dim_claimextension
                 where CLAIM_UNIQUEID = ?
                   and valid_fromdate > ?
                 order by valid_fromdate asc)
         where rownum &lt; 2
         order by 1, 3)
 where rownum &lt; 2
</sql>
