    <sql>
select *
from (
select sc.clm_claimstatuscd    prd_clm_claimstatuscd,
               dcs.claimstatus_id      claimstatus_id,
               sc.clm_closeddate       prd_clm_closeddate,
               clsdt.closeddate_id     prd_closeddate_id,
               sc.clm_lossreporteddate prd_clm_lossreporteddate,
               rptdt.datereported_id   prd_datereported_id,
               sc.clm_dateofloss       prd_clm_dateofloss,
               lossdt.dateofloss_id    prd_dateofloss_id,
               adjuster_uniqueid       prd_adjuster_uniqueid,
               contact_uniqueid        prd_contact_uniqueid,
               sc.clm_city             prd_clm_city,
               sc.clm_county           prd_clm_county,
               sc.clm_state            prd_clm_state,
               sc.clm_postalcode       prd_clm_postalcode,
               sc.clm_country          prd_clm_country,
               sc.insured_uniqueid     insured_uniqueid,
               sc.claimant_uniqueid    claimant_uniqueid,
               sc.mmclm_current_claim_type,
               sc.mmclm_claim_subtype_code,
               sc.MMCLM_FINAL_DISPOSITION_CODE
          from stg_claim         sc,
               vdim_claimstatus  dcs,
               vdim_closedate    clsdt,
               vdim_dateofloss   lossdt,
               vdim_reporteddate rptdt
         where stgclaim_id                 = ?
           and sc.clm_claimstatuscd        = dcs.clmst_statuscd(+)
           and clsdt.clsdt_date            = sc.clm_closeddate
           and lossdt.dol_date             = sc.clm_dateofloss
           and rptdt.rptdt_date            = sc.clm_lossreporteddate)
where rownum = 1
</sql>
