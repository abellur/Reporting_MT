    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Get the minimum date

var dbStartDate;

dbStartDate = POLICYSTARTDATE;

// Get the maximum date

var dbEndDate;

dbEndDate = POLICYENDDATE;

// Calculate the number of months between
// the start date (sDate) and end date (eDate)

var months = dateDiff(dbStartDate,dbEndDate,"m")+1;

// Calcuate the first day of the month for the start date
var startMonth = month(dbStartDate);
var startYear = year(dbStartDate); 
// Somehow Javascript codes can not calulate startDate correctly for certain months,
// for example when the dbStartDate is in 4/1957, the startDate generated by Date
// constructor is 3/31/1957, not 4/1/1957. Moved the calculation of startDate to a
// Formula step

//var startDate = new Date(startYear, startMonth+1, 1);

// Calculate the last day of the month for the end date
var endMonth = month(dbEndDate); 
var endYear = year(dbEndDate);
var endDate = new Date(endYear, endMonth+1, 0);
if(!isNum(months))
   months = 0;
</jsScript_script>
