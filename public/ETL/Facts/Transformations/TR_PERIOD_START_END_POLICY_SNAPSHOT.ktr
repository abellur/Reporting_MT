    <connection>4SBI_Staging</connection>
    <rowlimit>0</rowlimit>
    <sql>
SELECT GREATEST(add_months(TRUNC(SYSDATE), (-12 * ${G_YEARS_OF_PM_SNAPSHOT_DATA})),
                NVL(MIN(PT_ACCOUNTINGDATE),SYSDATE)) AS POLICYSTARTDATE,
       GREATEST(NVL(MAX(PT_ACCOUNTINGDATE),SYSDATE),SYSDATE) AS POLICYENDDATE
FROM   STG_POLICYTRANSACTION
WHERE  PT_ACCOUNTINGDATE NOT IN (to_date('1/1/1900','mm/dd/yyyy'), 
                                 to_date('1/1/3000','mm/dd/yyyy'))
AND    LOADDATE = ${sql_loadDate}
</sql>
