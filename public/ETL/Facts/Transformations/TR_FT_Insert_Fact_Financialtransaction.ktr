    <sql>ALTER TABLE Fact_Financialtransaction NOLOGGING;

INSERT ${insertHint} INTO Fact_Financialtransaction


SELECT t.sk_recordnum + ${MAX_FINANCIALTRANSACTION_ID} FINANCIALTRANSACTION_ID,
       nvl(to_number(to_char(t.ft_accountingdate,'yyyymmdd')),'19000101') ACCOUNTINGDATE_ID,
	   t.effectivedate_id,
	   0 product_id,
	   nvl(tc.transactionissuingcompany_id,${UNK_COMPANY_ID})  transactionissuingcompany_id,
	   nvl(vbc.billingcompany_id,${UNK_COMPANY_ID}) billingcompany_id,
	   nvl(NVL2(dp1.sk_recordnum,dp1.policy_id,dp2.policy_id),${UNK_POLICY_ID}) policy_id,
	   nvl(NVL2(dpe1.sk_recordnum,dpe1.policyextension_id,dpe2.policyextension_id),
           ${UNK_POLICYEXTENSION_ID}) policyextension_id,
	   nvl(sp.policyeffectivedate_id,19000101) policyeffectivedate_id,
       nvl(sp.policyexpirationdate_id,19000101) policyexpirationdate_id,
	   t.bookeffectivedate_id,
       t.bookexpirationdate_id,
	   nvl(ftr.financialtransactionext_id,${UNK_FINANCIALTRANSACTIONEXTENSION_ID}) financialtransactionext_id,
	   nvl(dt.territory_id,${UNK_TERRITORY_ID}) territory_id,
	   nvl(dc.coverage_id,${UNK_COVERAGE_ID}) coverage_id,
	   nvl(dce.coverageextension_id,${UNK_COVERAGEEXTENSION_ID}) coverageextension_id,
	   nvl(t.coverageeffectivedate_id,19000101) coverageeffectivedate_id,
       nvl(t.coverageexpirationdate_id,19000101) coverageexpirationdate_id,
	   nvl(dcr.coveredrisk_id,${UNK_COVEREDRISK_ID}) PRIMARYRISK_ID,
       nvl(dcre.primaryriskextension_id,${UNK_COVEREDRISKEXTENSION_ID}) primaryriskextension_id,
	   nvl(dcs.classification_id,${UNK_CLASSIFICATION_ID}) classification_id,
	   nvl(dcm.claim_id,${UNK_CLAIM_ID}) claim_id,
	   nvl(fi.firstinsured_id,${UNK_INSURED_ID}) firstinsured_id,
	   nvl(duw.underwriter_id,${UNK_UNDERWRITER_ID}) underwriter_id,
	   nvl(prd.producer_id,${UNK_PRODUCER_ID}) producer_id,
       nvl(sprd.subproducer_id,${UNK_PRODUCER_ID}) subproducer_id,
	   nvl(vah.accountholder_id,${UNK_ACCOUNTHOLDER_ID}) accountholder_id,
	   t.duedate_id,
	   t.invoicedate_id,
       t.agingstartdate_id,
	   nvl(va1.account1_id,${UNK_ACCOUNT_ID}) account1_id,
       nvl(vae1.account1extension_id,${UNK_ACCOUNT_ID}) account1extension_id,
       ${UNK_ACCOUNT_ID} account2_id,
       ${UNK_ACCOUNT_ID} account2extension_id,
	   nvl(dsa.subaccount_id,${UNK_SUBACCOUNT_ID}) subaccount_id,
	   t.invoicetransactionstatus_id,
	   nvl(t.financialterms_id,${UNK_FINANCIALTERMS_ID}) financialterms_id,
	   nvl(t.financialtransactiontype_id,${UNK_FINANCIALTRANSACTIONTYPE_ID}) financialtransactiontype_id,
	   nvl(bat.batch_id,${UNK_BATCH_ID}) batch_id,
       nvl(dpd.paymentdetail_id,${UNK_PAYMENTDETAIL_ID}) paymentdetail_id,
       nvl(t.financialgroup_id,${UNK_FINANCIALGROUP_ID}) financialgroup_id,
	   nvl(t.transactionbasis_id,${UNK_TRANSACTIONBASIS_ID}) transactionbasis_id,
       nvl(t.invoice_id,${UNK_INVOICE_ID}) invoice_id,     
       nvl(fpt.policytransaction_id,0) policytransaction_id,
       nvl(fct.claimtransaction_id,0) claimtransaction_id,
	   nvl(t.ft_sequence,0) ft_sequence,
       NVL(n.policyneworrenewal,'Unknown') policyneworrenewal,
       NVL(t.originaltransactioncode,'Unknown') originaltransactioncode,
	   nvl(t.ft_referencenumber,0) ft_referencenumber,
	   NVL(t.transaction_source,'Unknown') transaction_source,
       NVL(t.ft_status,' ') ft_status,
	   nvl(t.ft_installmentnumber,0) ft_installmentnumber,
       nvl(t.ft_baseamount,0) ft_baseamount,
       nvl(t.ft_amount,0) ft_amount,
       t.account1_uniqueid,
       t.loaddate,
       -- Issue 178595, add unknown commission payment ID as default value, 
       --               and set default commission payment amount to be 0.
       0, 
       0,
       -- Issue 187138, add financial accounting date
       GREATEST(t.effectivedate_id,nvl(to_number(to_char(t.ft_accountingdate,'yyyymmdd')),'19000101')) financialaccountingdate_id
FROM   tmp_ftt_lkup_transissuecompany tc,
       tmp_ftt_lkup_producer          prd,
       tmp_ftt_lkup_subproducer       sprd,
       tmp_ftt_lkup_firstinsured      fi,
       tmp_ftt_lkup_coveredrisk       dcr,
       tmp_ftt_lkup_coveredriskext    dcre,
       tmp_ftt_lkup_territory         dt,
       tmp_ftt_lkup_dim_coverage      dc,
       tmp_ftt_lkup_dim_policy1       dp1,
       tmp_ftt_lkup_dim_policy2       dp2,
       tmp_ftt_lkup_underwriter       duw,
       tmp_ftt_lkup_classification    dcs,
       tmp_ftt_lkup_coverageext       dce,
       tmp_ftt_lkup_dim_policyext1    dpe1,
       tmp_ftt_lkup_dim_policyext2    dpe2,
       tmp_ftt_lkup_ftransext         ftr,
       tmp_ftt_lkup_claim             dcm,
       tmp_ftt_lkup_billcomp          vbc,
       tmp_ftt_lkup_accthldr          vah,
       tmp_ftt_lkup_account1          va1,
       tmp_ftt_lkup_accountext1       vae1,
       tmp_ftt_lkup_subaccount        dsa,
       tmp_ftt_lkup_batch             bat,
       tmp_ftt_lkup_paymentdetail     dpd,
       tmp_ftt_lkup_poltrans          fpt,
       tmp_ftt_lkup_clmtrans          fct,
       tmp_ftt_lkup_newrenewal        n,
       tmp_ftt_lkup_stg_policy        sp,
       tmp_ftt_trans_detail_1         t
WHERE  1 = 1
AND    t.sk_recordnum = tc.sk_recordnum(+)
AND    t.sk_recordnum = prd.sk_recordnum(+)      
AND    t.sk_recordnum = sprd.sk_recordnum(+)
AND    t.sk_recordnum = fi.sk_recordnum(+)
AND    t.sk_recordnum = dcr.sk_recordnum(+)
AND    t.sk_recordnum = dcre.sk_recordnum(+)
AND    t.sk_recordnum = dt.sk_recordnum(+)
AND    t.sk_recordnum = dc.sk_recordnum(+)
AND    t.sk_recordnum = dp1.sk_recordnum(+)
AND    t.sk_recordnum = dp2.sk_recordnum(+)
AND    t.sk_recordnum = duw.sk_recordnum(+)
AND    t.sk_recordnum = dcs.sk_recordnum(+)
AND    t.sk_recordnum = dce.sk_recordnum(+)
AND    t.sk_recordnum = dpe1.sk_recordnum(+)
AND    t.sk_recordnum = dpe2.sk_recordnum(+)
AND    t.sk_recordnum = ftr.sk_recordnum(+)
AND    t.sk_recordnum = dcm.sk_recordnum(+)
AND    t.sk_recordnum = vbc.sk_recordnum(+)
AND    t.sk_recordnum = vah.sk_recordnum(+)
AND    t.sk_recordnum = va1.sk_recordnum(+)
AND    t.sk_recordnum = vae1.sk_recordnum(+)
AND    t.sk_recordnum = dsa.sk_recordnum(+)
AND    t.sk_recordnum = bat.sk_recordnum(+)
AND    t.sk_recordnum = dpd.sk_recordnum(+)
AND    t.sk_recordnum = fpt.sk_recordnum(+)
AND    t.sk_recordnum = fct.sk_recordnum(+)
AND    t.sk_recordnum = n.sk_recordnum(+)
AND    t.sk_recordnum = sp.sk_recordnum(+);

commit;</sql>
