    <sql>insert /*+ append */
into fact_claimtransaction with txn_info as
  (select /*+ full */
    claimtransaction_id       txn_id,
    claimtransaction_uniqueid txn_uid,
    transactionstatus_id      stat_id,
    -- Issue 184044
    transactionstatusdate
     from fact_claimtransaction t
    where transactionflag in ('U','N','I')
      and loadDate = ${sql_loadDate})
  select txn_id - 1 claimtransaction_id, 
         transactiondate_id,
         accountingdate_id,
         claimtransactiontype_id,
         coverage_id,
         coverageextension_id,
         coverageeffectivedate_id,
         coverageexpirationdate_id,
         coveredrisk_id,
         coveredriskextension_id,
         coveredrisklocation_id,
         adjuster_id,
         claimant_id,
         underwriter_id,
         producer_id,
         product_id,
         company_id,
         firstinsured_id,
         secondinsured_id,
         thirdinsured_id,
         firstotherinterest_id,
         secondotherinterest_id,
         payee_id,
         contact_id,
         claim_id,
         claimextension_id,
         claimstatus_id,
         claimlosslocation_id,
         policy_id,
         policyextension_id,
         policyeffectivedate_id,
         policyexpirationdate_id,
         limit_id,
         deductible_id,
         class_id,
         closeddate_id,
         datereported_id,
         dateofloss_id,
         territory_id,
         claimtransactionextension_id,
         losscoding_id,
         claimtype_id,
         finaldisposition_id,
         claimtransaction_uniqueid,
         'O' transactionflag,
         transactionstatus,
         transactionrollup,
         -amount amount,
         ${sql_loadDate} loaddate,
         -- Issue 184044
         txn.transactionstatusdate,
         transactionstatus_id,
         clmpart_id1,
         clmpart_id2,
         clmpart_id3,
         clmpart_id4,
         clmpart_id5,
         clmpart_id6,
         clmpart_id7,
         clmpart_id8,
         clmpart_id9,
         clmpart_id10,
         claim_uniqueid,
         opendate_id, 
         courtcase_id,
         -- Issue 178894
         claimpiaadata_id,
         occurrence_id,
         clmpart_id11,
         clmpart_id12,
         clmpart_id13,
         clmpart_id14,
         clmpart_id15
    from fact_claimtransaction ft,
         txn_info              txn
   where claimtransaction_id =
         (select max(claimtransaction_id)
            from fact_claimtransaction fc
           where claimtransaction_uniqueid = txn.txn_uid
             and transactionFlag          &lt;&gt; 'O'
             and claimtransaction_id       &lt; txn.txn_id);
commit;
</sql>
