    <entry>
      <name>Create tmp clm ext id</name>
      <description/>
      <type>SQL</type>
      <sql>create table tmp_clm_mon_sum_clm_ext_id nologging pctfree 0
       tablespace ${G_STAGING_DB_DATA} as
select c.claim_uniqueid,
       max(claimextension_id) claimextension_id
  from tmp_clm_mon_sum_clm_uid c,
       dim_claimextension dc
 where dc.claim_uniqueid = c.claim_uniqueid
   and valid_fromdate &lt;= ${sql_periodEndDate}
   and valid_todate   >  ${sql_periodEndDate}
   group by c.claim_uniqueid;
   
-- create claim id where we only have dim claim rows created after this period.   
insert
  into tmp_clm_mon_sum_clm_ext_id   
select dc.claim_uniqueid claim_uniqueid,
       min(claimextension_id) claimextension_id
  from (select claim_uniqueid
          from tmp_clm_mon_sum_clm_uid mc
         where not exists(select 'x'
                    from tmp_clm_mon_sum_clm_ext_id cid
                   where cid.claim_uniqueid = mc.claim_uniqueid)) v,
       dim_claimextension dc
 where dc.claim_uniqueid    = v.claim_uniqueid
   and dc.valid_fromdate > ${sql_periodEndDate}
   group by dc.claim_uniqueid;
commit;
</sql>
