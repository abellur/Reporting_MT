    <entry>
      <name>Create tmp clm calc</name>
      <description/>
      <type>SQL</type>
      <sql>create table tmp_clm_mon_sum_clm_calc nologging pctfree 0
       tablespace ${G_STAGING_DB_DATA} as
select fc.claim_uniqueid,
      nvl(v.loss_pd_amt, 0) loss_pd_amt, 
      nvl(v.loss_rsrv_chng_amt, 0) loss_rsrv_chng_amt, 
      nvl(v.alc_exp_pd_amt, 0) alc_exp_pd_amt, 
      nvl(v.alc_exp_rsrv_chng_amt, 0) alc_exp_rsrv_chng_amt, 
      nvl(v.ualc_exp_pd_amt, 0) ualc_exp_pd_amt, 
      nvl(v.ualc_exp_rsrv_chng_amt, 0) ualc_exp_rsrv_chng_amt, 
      nvl(v.subro_recv_chng_amt, 0) subro_recv_chng_amt, 
      nvl(v.subro_rsrv_chng_amt, 0) subro_rsrv_chng_amt, 
      nvl(v.subro_paid_chng_amt, 0) subro_paid_chng_amt, 
      nvl(v.salvage_recv_chng_amt, 0) salvage_recv_chng_amt, 
      nvl(v.salvage_rsrv_chng_amt, 0) salvage_rsrv_chng_amt, 
      nvl(v.dedrecov_recv_chng_amt, 0) dedrecov_recv_chng_amt, 
      nvl(v.dedrecov_rsrv_chng_amt, 0) dedrecov_rsrv_chng_amt, 
      nvl(v.alc_exp_recovery_amt, 0) alc_exp_recovery_amt, 
      nvl(v.loss_recovery_amt, 0) loss_recovery_amt, 
      nvl(v.other_recovery_amt, 0) other_recovery_amt, 
      nvl(v.usr_def_sum1, 0) usr_def_sum1, 
      nvl(v.usr_def_sum2, 0) usr_def_sum2, 
      nvl(v.usr_def_sum3, 0) usr_def_sum3, 
      nvl(v.usr_def_sum4, 0) usr_def_sum4, 
