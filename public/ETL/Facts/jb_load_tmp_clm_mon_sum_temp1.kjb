      <type>SQL</type>
      <sql>create table tmp_clm_mon_sum_stgclm_id nologging pctfree 0
       tablespace ${G_STAGING_DB_DATA} as
select sc.claim_uniqueid,  
       max(stgclaim_id) max_stgclaim_id
  from stg_claim sc,
       tmp_clm_mon_sum_clm_uid c
 where sc.claim_uniqueid = c.claim_uniqueid
   and sc.clm_changeeffectivedate &lt;= ${sql_periodEndDate}
   group by sc.claim_uniqueid;


insert
  into tmp_clm_mon_sum_stgclm_id   
select sc.claim_uniqueid,
       min(stgclaim_id)   
  from (select claim_uniqueid
          from tmp_clm_mon_sum_clm_uid mc
         where not exists(select 'x'
                    from tmp_clm_mon_sum_stgclm_id cid
                   where cid.claim_uniqueid = mc.claim_uniqueid)) v,
     stg_claim sc
where sc.claim_uniqueid = v.claim_uniqueid
group by sc.claim_uniqueid;

commit;

create table tmp_clm_mon_sum_clm_stg
       nologging pctfree 0
       tablespace ${G_STAGING_DB_DATA} as
select sc.claim_uniqueid,
       sc.clm_claimstatuscd, 
       sc.clm_closeddate,
       nvl(to_number(to_char(clm_closeddate,'yyyymmdd')),19000101) closeddate_id,
       sc.clm_lossreporteddate ,
       nvl(to_number(to_char(clm_lossreporteddate,'yyyymmdd')),19000101) datereported_id   ,
       sc.clm_dateofloss       ,
       nvl(to_number(to_char(clm_dateofloss,'yyyymmdd')),19000101)  dateofloss_id    ,
       adjuster_uniqueid       ,
       contact_uniqueid        ,
       sc.clm_city             ,
       sc.clm_county           ,
       sc.clm_state            ,
       sc.clm_postalcode       ,
       sc.clm_country          ,
       sc.insured_uniqueid     ,
       sc.claimant_uniqueid    ,
       sc.mmclm_current_claim_type,
       sc.mmclm_claim_subtype_code,
       sc.MMCLM_FINAL_DISPOSITION_CODE,
       (select claimtype_id
          from dim_claimtype dct
         where dct.ctyp_code = sc.mmclm_current_claim_type) claimtype_id,
       (select claimstatus_id
          from vdim_claimstatus s
         where s.CLMST_STATUSCD = sc.clm_claimstatuscd) claimstatus_id ,
       (select finaldisposition_id
          from dim_finaldisposition fd
         where fd.finaldisp_code =  sc.mmclm_final_disposition_code) finaldisposition_id,
       NVL(vop.opendate_id,19000101)     opendate_id, 
       NVL(dctt.courtcase_id,0)          courtcase_id,
       -- Issue #178894
       NVL(dpd.claimpiaadata_id,0)       claimpiaadata_id
  from tmp_clm_mon_sum_stgclm_id t,
       stg_claim         sc,       
       dim_courtcase     dctt,
       vdim_opendate     vop,
       DIM_CLAIMPIAADATA     dpd
 where stgclaim_id        = t.max_stgclaim_id
   AND sc.clm_claimnumber = dctt.source_number(+)
   AND sc.clm_opendate    = vop.open_date(+)   
   AND sc.clm_claimnumber = dpd.source_claimnumber(+)
;</sql>
