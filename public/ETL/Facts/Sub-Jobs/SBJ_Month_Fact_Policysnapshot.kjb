    <entry>
      <name>Create tmp_fps_lkup_polstat_id</name>
      <description/>
      <type>SQL</type>
      <sql>create table tmp_fps_lkup_polstat_id nologging pctfree 0 
tablespace ${G_DW_DB_DATA} AS
WITH x AS (
     SELECT t.sk,
            CASE 
              WHEN s.stat_status = 'Cancelled' THEN
                'CN'
              -- Issue 194929. If policy expiration date equal to the least of current month's end date or load date, 
              -- then treat it as Expired
              WHEN t.policyexpirationdate_id &lt;= least(t.periodenddate,to_number(to_char(${sql_loadDate},'yyyymmdd'))) THEN 
                'EXP'
              WHEN t.policyexpirationdate_id > least(t.periodenddate,to_number(to_char(${sql_loadDate},'yyyymmdd'))) AND 
                   t.policyeffectivedate_id &lt;= least(t.periodenddate,to_number(to_char(${sql_loadDate},'yyyymmdd'))) THEN
                'INF'
              ELSE
                'UNK'
            END policy_status
     FROM   tmp_fps_main_table t, 
            dim_status s
     WHERE  t.policystatus_id = s.status_id)
SELECT x.sk, nvl(vps.policystatus_id,1) policystatus_id
FROM   vdim_policystatus vps, x
WHERE  x.policy_status = vps.polst_statuscd(+)
;
</sql>
