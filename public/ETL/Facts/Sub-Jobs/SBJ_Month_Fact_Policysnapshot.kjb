    </entry>
    <entry>
      <name>Create tmp_fps_main_table</name>
      <description/>
      <type>SQL</type>
      <sql>CREATE TABLE tmp_fps_main_table NOLOGGING PCTFREE 0 
tablespace ${G_DW_DB_DATA} AS
SELECT /*+ use_hash(fpc t) */
       rownum sk, 
       to_number(to_char(${sql_periodEndDate},'yyyymmdd')) periodenddate, 
       to_number(to_char(${sql_periodStartDate},'yyyymmdd')) periodstartdate, 
       fpc.month_id,
       fpc.underwriter_id,
       fpc.producer_id, 
       fpc.product_id,
       fpc.company_id,
       fpc.firstinsured_id,
       fpc.policy_id, 
       fpc.policyextension_id,
       fpc.policyeffectivedate_id, 
       fpc.policyexpirationdate_id,
       fpc.policystatus_id,
