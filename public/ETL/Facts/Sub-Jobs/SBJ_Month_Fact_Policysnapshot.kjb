    <entry>
      <name>Create tmp_fps_max_month_id Table</name>
      <description/>
      <type>SQL</type>
      <sql>-- Get max month_id for each coverage_uniqueid.
CREATE TABLE tmp_fps_max_month_id NOLOGGING PCTFREE 0 
tablespace ${G_DW_DB_DATA} AS
SELECT dp.pol_uniqueid, 
       dce.cov_uniqueid,
       MAX(fpc.month_id) max_month_id
FROM   fact_policycoverage fpc, 
       dim_coverageextension dce, 
       dim_policy          dp
WHERE  fpc.coverageextension_id = dce.coverageextension_id
AND    fpc.policy_id = dp.policy_id
AND    fpc.month_id &lt;= ${month_id}
GROUP  BY dp.pol_uniqueid, dce.cov_uniqueid    
;
</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>4SBI_DataWarehouse</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
