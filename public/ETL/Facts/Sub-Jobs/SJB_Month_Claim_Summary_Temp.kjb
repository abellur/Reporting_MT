    <entry>
      <name>SQL: Remove Deleted UID</name>
      <description>Deletes the current month's (using month_id variable) fact records from the fact_claim table so that they can be recreated and inserted.</description>
      <type>SQL</type>
      <sql>delete
  from tmp_clm_mon_sum_clm_uid t
where  ('Y' = (select nvl(clm_featuredeleteflag,'N')
                from dim_claim dc1
               where dc1.claim_id = (select max(claim_id)
                                       from dim_claim dc2
                                      where dc2.clm_uniqueid = t.claim_uniqueid
                                       and least(${sql_periodEndDate}, dc2.valid_todate) >=
                                           greatest(${sql_periodStartDate},dc2.valid_fromdate))));

commit;</sql>
