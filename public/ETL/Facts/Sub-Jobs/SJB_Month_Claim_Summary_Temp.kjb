    <entry>
      <name>Create tmp sum uid</name>
      <description/>
      <type>SQL</type>
      <sql>create table tmp_clm_mon_sum_clm_uid  pctfree 0 nologging
tablespace ${G_STAGING_DB_DATA}
as
select fc.claim_uniqueid,
       to_number(substr(fc.claim_uniqueid,1,instr(fc.claim_uniqueid,'^') - 1)) claim_fk,
       min_claimtransaction_id,
       'N' new_b
  from fact_claim fc,
       tmp_clm_mon_sum_mon_precalc pre
  where fc.claim_uniqueid = pre.claim_uniqueid
    and fc.month_id = ${priorMonth_id}
union all
select ft.claim_uniqueid,
       to_number(substr(ft.claim_uniqueid,1,instr(ft.claim_uniqueid,'^') - 1)) claim_fk,
       min_claimtransaction_id,
       'Y'
  from tmp_clm_mon_sum_clm_precalc ft
  where ft.month_id = ${month_id}
  and not exists(select 'x'
                   from fact_claim fc1
                  where fc1.claim_uniqueid = ft.claim_uniqueid
                    and fc1.month_id = ${priorMonth_id})
  ;</sql>
