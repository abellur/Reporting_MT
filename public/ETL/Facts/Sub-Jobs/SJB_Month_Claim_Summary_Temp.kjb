      <description/>
      <type>SQL</type>
      <sql>create table tmp_clm_mon_sum_mon_precalc pctfree 0 nologging
tablespace ${G_STAGING_DB_DATA}
as
select claim_uniqueid,
       min(min_claimtransaction_id) min_claimtransaction_id
 from tmp_clm_mon_sum_clm_precalc
where month_id &lt;= ${priorMonth_id} 
group by claim_uniqueid
  ;</sql>
