    <entry>
      <name>Create tmp_fc_osr_driver</name>
      <description/>
      <type>SQL</type>
      <sql>CREATE TABLE tmp_fc_osr_driver NOLOGGING PCTFREE 0 
tablespace ${G_STAGING_DB_DATA} AS
WITH x AS (
SELECT fc.month_id, 
       fc.adjuster_id,
       --fc.underwriter_id,
       --fc.company_id,
       dc.clm_claimnumber,
       /* fc.policy_id,
       fc.policyextension_id,
       fc.policyeffectivedate_id,
       fc.policyexpirationdate_id,  */     
       MAX(fc.claim_id) claim_id,
       MAX(fc.claimextension_id) claimextension_id,
       fc.claimstatus_id,
       fc.claimlosslocation_id,
       fc.datereported_id,
       fc.dateofloss_id,
       fc.closeddate_id,
       fc.opendate_id,
       fc.courtcase_id,
       -- Issue 178894
       fc.claimpiaadata_id,
       MAX(fc.losscoding_id) losscoding_id,
       fc.claimtype_id,
       fc.finaldisposition_id,
       fc.clmpart_id1,
       fc.clmpart_id2,
       fc.clmpart_id3,
       fc.clmpart_id4,
       fc.clmpart_id5,
       fc.clmpart_id6,
       fc.clmpart_id7,
       fc.clmpart_id8,
       fc.clmpart_id9,
       fc.clmpart_id10,
       fc.clmpart_id11,
       fc.clmpart_id12,
       fc.clmpart_id13,
       fc.clmpart_id14,
       fc.clmpart_id15,
       --claimuniqueid
