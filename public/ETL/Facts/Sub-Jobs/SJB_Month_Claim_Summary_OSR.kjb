      <description/>
      <type>SQL</type>
      <sql>CREATE TABLE tmp_fc_osr_stg_policy NOLOGGING PCTFREE 0 
tablespace ${G_STAGING_DB_DATA} AS
WITH x AS 
       (SELECT t.sk,
               sp.policy_uniqueid,
               sp.underwriter_uniqueid, 
               sp.company_uniqueid,
               sp.pol_effectivedate,
               sp.pol_expirationdate,
               row_number() over (PARTITION BY t.sk order by sp.stgpolicy_id desc) pick1
        FROM   tmp_fc_osr_stg_claim t,
               stg_policy sp
        WHERE  t.policy_uniqueid = sp.policy_uniqueid)
SELECT sk,
       policy_uniqueid,
       underwriter_uniqueid,
       company_uniqueid,
       to_number(to_char(pol_effectivedate,'yyyymmdd')) policyeffectivedate_id,
       to_number(to_char(pol_expirationdate,'yyyymmdd')) policyexpirationdate_id
FROM   x
WHERE  pick1 = 1;
</sql>
