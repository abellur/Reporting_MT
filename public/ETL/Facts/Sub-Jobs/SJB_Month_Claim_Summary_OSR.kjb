    <entry>
      <name>Create tmp_fc_osr_stg_claim</name>
      <description/>
      <type>SQL</type>
      <sql>CREATE TABLE tmp_fc_osr_stg_claim NOLOGGING PCTFREE 0
tablespace ${G_STAGING_DB_DATA} AS
WITH x AS 
       (SELECT d.sk,
               dc.clm_uniqueid claim_uniqueid,
               sc.insured_uniqueid,
               sc.claimant_uniqueid,
               sc.policy_uniqueid,               
               row_number() OVER (PARTITION BY d.sk ORDER BY sc.claim_insured_fk, sc.claimant_fk, sc.stgclaim_id) pick1
        FROM   tmp_fc_osr_driver d,
               dim_claim         dc,
               stg_claim         sc
        WHERE  d.clm_claimnumber = dc.clm_claimnumber
        AND    sc.claim_uniqueid = dc.clm_uniqueid
        AND    sc.mmclm_clmnt_primary_b = 'Y'
        and    sc.policy_uniqueid is not null)
SELECT x.sk,
       x.claim_uniqueid,
       x.insured_uniqueid,
       x.claimant_uniqueid,
       x.policy_uniqueid
FROM   x 
WHERE  x.pick1 = 1;

INSERT INTO tmp_fc_osr_stg_claim 
WITH x AS 
       (SELECT d.sk,
               dc.clm_uniqueid claim_uniqueid,
               sc.insured_uniqueid,
               sc.claimant_uniqueid,
               sc.policy_uniqueid,               
               row_number() OVER (PARTITION BY d.sk ORDER BY sc.claim_insured_fk, sc.claimant_fk, sc.stgclaim_id) pick1
        FROM   tmp_fc_osr_driver d,
               dim_claim         dc,
               stg_claim         sc
        WHERE  d.clm_claimnumber = dc.clm_claimnumber
        AND    sc.claim_uniqueid = dc.clm_uniqueid
        and    not exists
               (select 1 
                from   tmp_fc_osr_stg_claim t
                where  d.sk = t.sk)
       )
SELECT x.sk,
       x.claim_uniqueid,
       x.insured_uniqueid,
       x.claimant_uniqueid,
       x.policy_uniqueid
FROM   x 
WHERE  x.pick1 = 1;

commit;</sql>
