      <name>Create tmp_fpc_wk_union2_t1</name>
      <description/>
      <type>SQL</type>
      <sql>--     Transactions effective between the beginning date of period year and period end date, 
--     or transaction accounting date is between the beginning date of period year and period end date
--     but Policy term expiration date is before current period end date.
create table tmp_fpc_wk_union2_t1 nologging pctfree 0 tablespace ${G_STAGING_DB_DATA}
as
SELECT pt1.pol_uniqueid
  FROM tmp_fpc_fact_policytrans pt1, tmp_fpc_genmonths m1
 WHERE 1 = 1
   AND pt1.policyexpirationdate_id &lt;= m1.sql_periodstartdate_id
   AND pt1.coverageeffectivedate_id &lt;= m1.sql_periodenddate_id
   AND pt1.effectivedate_id &lt;= m1.sql_periodenddate_id
   AND to_number(substr(to_char(pt1.effectivedate_id), 1, 4)) = m1.periodyear
UNION
SELECT pt1.pol_uniqueid
  FROM tmp_fpc_fact_policytrans pt1, tmp_fpc_genmonths m1
 WHERE 1 = 1
   AND pt1.policyexpirationdate_id &lt;= m1.sql_periodstartdate_id
   AND pt1.coverageeffectivedate_id &lt;= m1.sql_periodenddate_id
   AND pt1.effectivedate_id &lt;= m1.sql_periodenddate_id
   AND pt1.accountingdate_id &lt;= m1.sql_periodenddate_id
   AND to_number(substr(to_char(pt1.accountingdate_id), 1, 4)) = m1.periodyear;</sql>
