  <entries>
    <entry>
      <name>Create tmp_fpc_org_pol_expdt_id </name>
      <description/>
      <type>SQL</type>
      <sql>create table tmp_fpc_org_pol_expdt_id 
nologging 
tablespace ${G_STAGING_DB_DATA}
as
SELECT dpe1.policy_uniqueid, MAX(to_number(to_char(dpe1.pol_exp_date,'YYYYMMDD'))) org_pol_expdate_id
FROM   DIM_POLICYEXTENSION dpe1
GROUP  BY dpe1.policy_uniqueid
;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>4SBI_Staging</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>344</xloc>
      <yloc>24</yloc>
    </entry>
    <entry>
      <name>Create tmp_fpc_fact_policytrans</name>
      <description/>
      <type>SQL</type>
      <sql>create table tmp_fpc_fact_policytrans 
nologging 
tablespace ${G_STAGING_DB_DATA}
as
select /*+ use_hash (f cex prisk dcm doa) */
    ROWNUM sk_tmp_fpc_policytrans,
    tt.trans_code,
    dpe.MMPOL_POLICY_CURRENT_STATUS,
    dp.POL_UNIQUEID,
    cex.COV_UNIQUEID,
    cex.MMCOV_COVERAGE_SEGMENT_CODE,
    PRISK.CVRSK_UNIQUEID,
    x.org_pol_expdate_id,
    case when f.POLICYEXPIRATIONDATE_ID=0 then 1900 else to_number(substr(to_char(f.POLICYEXPIRATIONDATE_ID),1,4)) end POLEXP_YEAR,
    case when f.INCLUDE_IN_SUMMARY_B = 'Y' AND f.COVERAGE_ID &lt;&gt; 0 then 'Y' else 'N' end unionLogic_flg,
    DCM.COMPONENT_UNIQUEID,
    DOA.OTHERADJUSTMENT_UNIQUEID,
    f.*
from 
    fact_policytransaction f,
    DIM_TRANSACTIONTYPE tt,
    DIM_POLICYEXTENSION DPE,
    DIM_POLICY dp,
    DIM_COVERAGEEXTENSION cex,
    DIM_COVEREDRISK PRISK,
    DIM_COMPONENT DCM,
    DIM_OTHER_ADJUSTMENT DOA,
    tmp_fpc_org_pol_expdt_id x 
where 
       f.TRANSACTIONTYPE_ID = tt.TRANSACTIONTYPE_ID
AND    f.POLICYEXTENSION_ID = DPE.POLICYEXTENSION_ID
AND    f.POLICY_ID = dp.POLICY_ID
AND    f.COVERAGEEXTENSION_ID = cex.COVERAGEEXTENSION_ID
