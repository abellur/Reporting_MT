      <name>Create tmp_fpc_polcancel</name>
      <description/>
      <type>SQL</type>
      <sql>create table tmp_fpc_polcancel nologging tablespace ${G_STAGING_DB_DATA}
as
select rownum sk_polcancel, fp.*
from
(
	select fp.policy_id,
		   fp.transactiondate_id,
		   fp.transactionsequence,
		   fp.can_trans_cd,
		   fp.effectivedate_id,
		   fp.can_eff_date
	from
	(
		select 
		    fp.policy_id,
		    fp.transactiondate_id,
		    fp.transactionsequence,
		    tt.trans_code as can_trans_cd,
		    fp.effectivedate_id,
		    to_date(fp.effectivedate_id,'YYYYMMDD') can_eff_date,
	        row_number() over (partition by fp.policy_id order by fp.transactiondate_id desc,fp.effectivedate_id desc) pick1
		from fact_policytransaction fp,
		     dim_transactiontype tt
		where fp.transactiontype_id = tt.transactiontype_id
        and fp.source_system = 'OASIS'
		and tt.trans_code in ('CM', 'CF', 'CS', 'CR', 'RI')
	) fp
	where fp.pick1 = 1
) fp
;
</sql>
