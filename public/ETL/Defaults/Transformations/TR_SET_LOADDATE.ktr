        <jsScript_script>//  Set the load date based on the database date format needed.

var sql_loadDate;
var sql_defaultDate;

var tmp_database = getVariable("G_DATABASE", "MSSQL");

// ** MSSQL **
if (tmp_database == "MSSQL") {
	sql_loadDate = "'" + date2str(loadDate,"yyyy/MM/dd HH:mm:ss") + "'";
	sql_defaultDate = "'" + date2str(var_defaultDate,"MM/dd/yyyy") + "'";
}

// ** Oracle **
if (tmp_database == "ORACLE") {
	sql_loadDate = "to_date('" + date2str(loadDate,"yyyy/MM/dd HH:mm:ss") + "'" +
	               ",'yyyy/mm/dd hh24:mi:ss')";
	sql_defaultDate = "to_date('" + date2str(var_defaultDate,"MM/dd/yyyy") + "'" +
	               ",'MM/dd/yyyy')";

}





