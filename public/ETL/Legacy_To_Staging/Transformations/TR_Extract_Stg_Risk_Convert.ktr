    <connection>Delphi_Convert_Legacy</connection>
    <sql>select DW_LEGACY_LOAD.GET_LEGACY_ID(cr.risk_key,'RISK',cr.risk_eff_from_date) risk_key,
	   DW_LEGACY_LOAD.get_legacy_id(cr.risk_key,'RISK',cr.risk_eff_from_date) risk_unid,
       DW_LEGACY_LOAD.get_risk_type(cr.risk_type_code,'RSK_CODE') risk_type_code,
       cr.risk_eff_from_date,
       cr.risk_eff_to_date,
       cr.risk_cancel_date,
	   DW_LEGACY_LOAD.get_legacy_id(cr.risk_client_key,'ENTITY') risk_client_key,
       cr.primary_risk_b,
       cr.state_code,
       (select max(cp1.TRANSACTION_EFF_DATE) from convert_policy_dwr cp1 where cp1.policy_no = cr.policy_no and cp1.transaction_key = cr.transaction_key) TRANSACTION_EFF_DATE,
       (select max(cp1.TRANSACTION_ACCT_DATE) from convert_policy_dwr cp1 where cp1.policy_no = cr.policy_no and cp1.transaction_key = cr.transaction_key) TRANSACTION_ACCT_DATE,       
       DW_LEGACY_LOAD.get_county(cr.county_code,cr.state_code,'COUNTY_CODE') county_code,
	   cr.territory_code,
       DW_LEGACY_LOAD.get_territory(cr.territory_code,cr.state_code,'TERRITORY_LONG_DESC') territory_desc,
       DW_LEGACY_LOAD.get_entity_address(cr.risk_client_key,'ADDRESS1') address1,
       DW_LEGACY_LOAD.get_entity_address(cr.risk_client_key,'ADDRESS2') address2,
       DW_LEGACY_LOAD.get_entity_address(cr.risk_client_key,'ADDRESS3') address3,
       DW_LEGACY_LOAD.get_entity_address(cr.risk_client_key,'COUNTY') risk_county_code,
       DW_LEGACY_LOAD.get_entity_address(cr.risk_client_key,'CITY') city,
	   DW_LEGACY_LOAD.get_entity_address(cr.risk_client_key,'STATE') risk_state_code,
       DW_LEGACY_LOAD.get_entity_address(cr.risk_client_key,'ZIP') zip,
       DW_LEGACY_LOAD.get_entity_address(cr.risk_client_key,'COUNTRY') risk_country_code,
       DW_LEGACY_LOAD.get_specialty_code(cr.specialty_code ,cr.state_code,'OASIS_SP') specialty_code,
       DW_LEGACY_LOAD.get_specialty_code(cr.specialty_code ,cr.state_code,'OASIS_lONG_DESC') specialty_desc,       
       cr.gross_premium risk_gross,
       cr.component_premium risk_comp_premium,
       cr.net_premium risk_net_premium,       
       trunc(sysdate) EXTRACT_DATE,
	   ' ' dummy_field,
	   0   dummy_num,
	   cr.convert_risk_pk pt_sequence,
       'A'	Rsk_transaction,
       'ACTIVE' current_status,
       decode(cr.active_risk_b,'A','ACTIVE','CANCEL') pol_rel_stat_type_code,
       decode(cr.active_risk_b,'A',null,
       (SELECT cc.coverage_cancel_reason
          FROM convert_coverage cc 
         WHERE cc.risk_key = cr.risk_key 
           AND cc.transaction_key = cr.transaction_key 
           AND cc.primary_coverage_b = 'P' 
           AND cc.destination='DWR'
           AND ROWNUM = 1)) pol_rel_reason_code,
       cr.policy_no,
       DW_LEGACY_LOAD.GET_LEGACY_ID(cr.transaction_key,'TRANS') transaction_key,
	   (select cp.convert_policy_pk 
        from convert_policy_dwr cp 
        where cp.transaction_key = cr.transaction_key
        AND   ROWNUM = 1) pt_key
  from convert_risk_dwr cr
where  1 = 1
--and    cr.risk_eff_from_date BETWEEN to_date('1/1/2010','mm/dd/yyyy') AND to_date('12/31/2011','mm/dd/yyyy')
--and    cr.policy_no = '1-116159'</sql>
