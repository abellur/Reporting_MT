    <connection>Delphi_Convert_Legacy</connection>
    <sql>SELECT  
       DW_LEGACY_LOAD.get_legacy_id(cc.coverage_key,'COV',cc.coverage_eff_from_date) coverage_key,
       DW_LEGACY_LOAD.get_coverage_code(cc.coverage_code,cc.risk_key,cc.coverage_form,'OASIS_CODE') coverage_code,
       cc.coverage_eff_from_date,
       cc.coverage_eff_to_date,
       cc.coverage_cancel_date,
       cc.primary_coverage_b,
       cc.coverage_form,
       cc.coverage_retro_date,
       DW_LEGACY_LOAD.get_coverage_limit(cc.coverage_limit_code,'OASIS_CODE') coverage_limit_code,
       cc.shared_limit_b,
       nvl((select max(cp1.TRANSACTION_EFF_DATE) from convert_policy_dwr cp1 where cp1.transaction_key = cc.transaction_key),cc.coverage_eff_from_date) TRANSACTION_EFF_DATE,
       (select max(cp1.TRANSACTION_ACCT_DATE) from convert_policy_dwr cp1 where cp1.transaction_key = cc.transaction_key) TRANSACTION_ACCT_DATE, 
       (select max(cp1.transaction_code1) from convert_policy_dwr cp1 where cp1.transaction_key = cc.transaction_key) TRANSACTION_CODE1,      
       decode(cc.tail_b,'Y','TAIL','N','MAIN',cc.tail_b) tail_b,
       decode(active_coverage_b,'Y','ACTIVE','CANCEL') coverage_status,
       cc.gross_premium coverage_gross,
       cc.component_premium coverage_comp_premium,
       cc.net_premium coverage_net_premium,
       cc.annual_premium,
       trunc(sysdate) EXTRACT_DATE,
       ' ' dummy_field,
       0   dummy_num,
       cc.convert_coverage_pk pt_sequence,
       'A' Cov_transaction,
       'INCIDENT' cov_limit1type,
       'AGGREGATE' cov_limit2type,
       DW_LEGACY_LOAD.get_coverage_limit(cc.coverage_limit_code,'OASIS_INCIDENT') cov_limit1,
       DW_LEGACY_LOAD.get_coverage_limit(cc.coverage_limit_code,'OASIS_AGGREATE') cov_limit2,
       cc.policy_no,
       DW_LEGACY_LOAD.GET_LEGACY_ID(cc.transaction_key,'TRANS') transaction_key,
       (select cp.convert_policy_pk from convert_policy_dwr cp where cp.transaction_key = cc.transaction_key) pt_key,
       'GROSS' comp_code,
       'GROSS' comp_short,
       'GROSS' comp_long,
       cc.coverage_code cov_code,
	   cccd.short_description cov_short,
	   cccd.long_description cov_long,
       cc.convert_coverage_pk source_pk
  from convert_coverage_dwr cc,  Convert_coverage_code cccd
 WHERE DW_LEGACY_LOAD.get_coverage_code(cc.coverage_code,cc.risk_key,cc.coverage_form,'OASIS_CODE') = cccd.oasis_product_coverage_code
 --  AND cc.policy_no = '1-116159'
 
