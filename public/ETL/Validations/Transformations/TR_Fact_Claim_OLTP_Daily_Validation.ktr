    <sql>select claim_no,
       exp_res,
       exp_pd,
       ind_res,
       ind_pd,
       'DW Totals: Claim_no = ' ||claim_no||
       ', exp_res = '           ||exp_res ||
       ', exp_pd  = '           ||exp_pd  ||
       ', ind_res = '           ||ind_res ||
       ', ind_pd  = '           ||ind_pd  ||' ' msg       
  from (

select clm_claimnumber claim_no,
       sum(alc_exp_rsrv_chng_amt_itd) exp_res,
       sum(alc_exp_pd_amt_itd)        exp_pd,
       sum(loss_rsrv_chng_amt_itd)    ind_res,
       sum(loss_pd_amt_itd)           ind_pd
  from temp_fact_claim_daily_val val,
       fact_claim fc,
       dim_claim dc
 where month_id+0         = (select max(month_id) 
                               from fact_claim)
   and val.claim_uniqueid = fc.claim_uniqueid
   and dc.claim_id        = fc.claim_id
   group by dc.clm_claimnumber)
order by claim_no</sql>
