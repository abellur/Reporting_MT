  <step>
    <name>Database join - Get Policy Prem</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Delphi_OLTP</connection>
    <rowlimit>0</rowlimit>
    <sql>WITH x AS (
       SELECT p.policy_no, pfd.policy_term_fk, MAX(pfd.transaction_log_fk) max_trans_fk 
       FROM   policy_premium_net_v pfd,
              policy      p,
              policy_term_history pth,
              transaction_log tl              
       WHERE  p.policy_pk = tl.policy_fk
       AND    pfd.transaction_log_fk = tl.transaction_log_pk
       AND    tl.Transaction_Status_Code = 'COMPLETE'
       AND    p.Policy_Cycle_Code = 'POLICY'
       AND    pth.policy_term_history_pk =  pfd.POLICY_TERM_FK
       AND    greatest(tl.accounting_date,tl.effective_from_date,pth.effective_from_date) &lt;= ?
       GROUP  BY p.policy_no, pfd.policy_term_fk), 
     y AS (
       SELECT pp.policy_term_fk, 
              pp.transaction_log_fk, 
              SUM(pp.premium_amount) Written_Prem
       FROM   policy_premium_v pp
       WHERE  pp.active_premium_b = 'Y'
     -- Not use NETPREM records since DRA is using the sum of 
     -- GROSS and component records to calculate the net premium,
     -- and there are scenarios in OLTP that the sum of GROSS and
     -- component records do not match the sum of NETPREM records.
       AND    pp.coverage_component_code &lt;&gt; 'NETPREM'
       AND    pp.rate_period_type_code = 'TERM_COVG'
       GROUP  BY pp.policy_term_fk, pp.transaction_log_fk)
--Get WP per term from OLTP.
SELECT x.Policy_No,
       x.Policy_No || '^' || x.policy_term_fk Pol_UniqueID,
       y.Written_Prem
FROM   x, 
       y 
WHERE  y.policy_term_fk  = x.policy_term_fk
AND    y.transaction_log_fk = x.max_trans_fk</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>MAX_MONTH_ENDDATE</name>
        <type>Date</type>
      </field>
    </parameter>
