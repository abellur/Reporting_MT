    <connection>Velocity_Staging</connection>
    <sql>SELECT DISTINCT vslw.location_uniqueid,
       vslw.guid                    location_guid,
       vspv.policyversion_uniqueid,
       vslw.transaction_uniqueid,
       vspu.policyunit_uniqueid,
       vslw.city,
       vslw.cityoverride,
       vslw.county,
       vslw.countyoverride,
       vslw.numberofemployees,
       vslw.productspec,
       vslw.state,
       vslw.unitnumber,
       vslw.zipcode,
       vslw.zipcodeoverride,
       va.addr_address_line1        address_line1,
       va.addr_address_line2        address_line2,
       va.addr_address_line3        address_line3,
       va.addr_country_code         address_country,
       va.addr_county_name          address_county,
       va.addr_state_code           address_state,
       va.addr_province             address_province,
       va.addr_city                 address_city_us,
       va.addr_zipcode              address_postal_code_us,
       va.addr_zip_plus_four        address_postal_code_plus_four,
       vslw.refresh_id,
       vst.accounting_date          change_effective_date,
       '${G_VL_SOURCE_SYSTEM}' source_system
  FROM vvl_stg_location            vslw
 INNER JOIN vl_stg_transaction     vst
    ON vslw.transaction_uniqueid = vst.transaction_uniqueid
  LEFT JOIN vl_stg_policyversion     vspv
    ON vspv.policyversion_uniqueid = vslw.policyversion_uniqueid
   AND vspv.transaction_uniqueid = vslw.transaction_uniqueid
  LEFT JOIN vl_stg_policyunit        vspu
    ON vspu.policyversion_uniqueid = vslw.policyversion_uniqueid
   AND vspu.transaction_uniqueid = vslw.transaction_uniqueid
   AND vspu.policyunit_uniqueid = vslw.policyunit_uniqueid
  LEFT JOIN dim_address              va
    ON va.addr_address_pk  = vslw.addressid
 WHERE 1=1
   AND vslw.refresh_id = ${vl_refresh_id}</sql>
