  <step>
    <name>Load Velocity PAS CLIENT_CLASSIFICATION&amp;CLIENT...  Data</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Velocity_Staging</connection>
    <sql>select distinct NVL(vscc.classification,'UNK') lenty_role,
       vscc.effective_date role_effective_date,
       vscc.expiration_date role_expiration_date,
       vsc.client_uniqueid client_uniqueid,
       vsc.client_type client_type,
       vsc.client_id source_client_id,
       vsc.prefix prefix,
       vsc.first_name first_name,
       vsc.middle_name middle_name,
       decode(upper(vsc.client_type),'INDIVIDUAL',vsc. last_name,'ORGANIZATION',vsc.company_name,null)    last_name,
       vsc.sounds_like sounds_like,
       vsc.name_history name_history,
       decode(upper(vsc.client_type),'INDIVIDUAL',vsc.ssn,'ORGANIZATION',vsc.fein,null) ssn,
       vsc.suffix suffix,
       vsc.company_name company_name,
       vsc.type type,
       vsc.date_of_birth date_of_birth,
       vsc.date_established date_established,
       vsc.client_description client_description,
       vsc.website website,
       vsa.country country,
       vsa.county county,
       vsa.state state,
       vsa.province province,
       vsa.city_us city_us,
       vsa.city city,
       vsa.address_line1 address_line1,
       vsa.address_line2 address_line2,
       vsa.address_line3 address_line3,
       vsa.postal_code_us postal_code_us,
       vsa.postal_code postal_code,
       vsa.postal_code_plus_four postal_code_plus_four,
       vsa.contact_info contact_info,
       nvl2(vsp.area_code, vsp.area_code || '-', null) || vsp.phone_number ||nvl2(vsp.ext, vsp.ext || '-', null) TELEPHONE,
       nvl2(vsp.area_code, vsp.area_code || '-', null) || vsp.phone_number ||nvl2(vsp.ext, vsp.ext || '-', null) FAX_NUMBER,
       nvl2(vsp.area_code, vsp.area_code || '-', null) || vsp.phone_number ||nvl2(vsp.ext, vsp.ext || '-', null) CELLPHONE_NUMBER,
       decode(vse.email_type, 'Work', vse.location, null) EMAIL_WORK,
       decode(vse.email_type, 'Home', vse.location, null) EMAIL_HOME,
       decode(upper(vsc.client_type),'ORGANIZATION',vsc.company_name,
              nvl2(vsc.Last_name, vsc.Last_name || ', ', null) || vsc.First_name ||nvl2(vsc.Middle_name, ' ' || vsc.Middle_name, null)) FORMATTED_NAME,
       vsc.refresh_id,
       '${G_VL_SOURCE_SYSTEM}' source_system,
       vsa.address_uniqueid
  from VL_STG_CLIENT vsc
  left join VL_STG_CLIENTCLASSIFICATION vscc
    on vscc.client_uniqueid = vsc.client_uniqueid AND vscc.refresh_id = vsc.refresh_id
  left join VL_STG_ADDRESS vsa
    on vsc.client_uniqueid = vsa.client_uniqueid AND vsa.refresh_id = vsc.refresh_id
  left join VL_STG_PHONENUMBER vsp
    on vsc.client_uniqueid = vsp.client_uniqueid AND vsp.refresh_id = vsc.refresh_id
  left join VL_STG_EMAILADDRESS vse
    on vsc.client_uniqueid = vse.client_uniqueid AND vse.refresh_id = vsc.refresh_id
 where 1 = 1
   and vsc.refresh_id = ${vl_refresh_id}</sql>
