        <jsScript_script>var requestXml;
var filename;
var req_filename;
// Request XML generated by SOAP UI
//
// Change the request to match your needs.
// Inject arguments in the place of the question marks like this:
// 
// var eMail = "someone@example.com";
// request=...
//     &lt;ws:email>{eMail}&lt;/ws:email> ...;
// 
// where eMail is the name of a scoped JS variable.  That includes input fields.
//
// See also: https://developer.mozilla.org/En/E4X
// 
//

requestXml =&lt;soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:pol="http://www.delphi-tech.com/velocity/PolicyInquiryService">
   &lt;soapenv:Header/>
   &lt;soapenv:Body>
      &lt;pol:PolicyInquiryRequest>
         &lt;pol:MessageId>&lt;/pol:MessageId>
         &lt;pol:CorrelationId>&lt;/pol:CorrelationId>
         &lt;pol:UserId>&lt;/pol:UserId>
         &lt;!--1 or more repetitions:-->
         &lt;pol:PolicyInquiryRequestParameters>
            &lt;pol:PolicyInquiry>
               &lt;pol:PolicyNumberId>&lt;/pol:PolicyNumberId>
               &lt;pol:PolicyId>&lt;/pol:PolicyId>
               &lt;!--1 or more repetitions:-->
               &lt;pol:PolicyTerm>
                  &lt;pol:PolicyTermNumberId>&lt;/pol:PolicyTermNumberId>
                  &lt;!--1 or more repetitions:-->
                  &lt;pol:PolicyVersion>
                     &lt;pol:PolicyVersionNumberId>{v_polversion_uniqueid}&lt;/pol:PolicyVersionNumberId>
                  &lt;/pol:PolicyVersion>
               &lt;/pol:PolicyTerm>
            &lt;/pol:PolicyInquiry>
         &lt;/pol:PolicyInquiryRequestParameters>
         &lt;pol:PolicyInquiryResultParameters>
            &lt;pol:PremiumAccountingFilter>
               &lt;pol:TransactionNumberId>{v_trans_uniqueid}&lt;/pol:TransactionNumberId>
            &lt;/pol:PremiumAccountingFilter>
         &lt;/pol:PolicyInquiryResultParameters>
      &lt;/pol:PolicyInquiryRequest>
   &lt;/soapenv:Body>
&lt;/soapenv:Envelope>;



// Please do not change the format of the file, the TRANSACTION_UNIQUEID is used in other transformations.
filename = G_VL_WS_OUTPUT_PATH+"\\"+v_vl_ref_id+"\\"+WEBSERVICE_NAME+"-"+v_trans_uniqueid+"_"+date2str(sysdate,"yyyyMMddHHmmss")+".xml";
req_filename = G_VL_WS_OUTPUT_PATH+"\\"+v_vl_ref_id+"\\"+WEBSERVICE_NAME+"-"+v_trans_uniqueid+"_"+date2str(sysdate,"yyyyMMddHHmmss")+"_req.txt";</jsScript_script>
