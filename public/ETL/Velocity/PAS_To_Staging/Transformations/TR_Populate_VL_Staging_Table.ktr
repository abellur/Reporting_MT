  <step>
    <name>Table input - XML Mapping Data</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Velocity_Staging</connection>
    <sql>WITH x AS (
SELECT t.table_name,
       c.column_name,
       e.xml_path,
       REPLACE(m.loop_xpath,'{NAMESPACE}','${V_NAMESPACE}') loop_xpath,
       e.xml_data_type,
       e.xml_data_size,
       e.xml_data_scale,
       e.xml_data_format,
       'T' xml_source
FROM   vl_conf_xmlcolmapping  m,
       vl_conf_columndefinition c,
       vl_conf_tabledefinition  t,
       vl_conf_xmlelement     e
WHERE  m.columndefinition_id = c.columndefinition_id
AND    m.xmlelement_id = e.xmlelement_id(+)
AND    c.tabledefinition_id = t.tabledefinition_id
AND    NOT EXISTS 
       (SELECT 1
        FROM   vl_conf_xmlcolmapoverride r
        WHERE  r.table_name = t.table_name
        AND    r.column_name = c.column_name)
UNION ALL 
SELECT r.table_name,
       r.column_name,
       REPLACE(r.xml_path,'{NAMESPACE}',r.namespace) xml_path,
       REPLACE(r.loop_xpath,'{NAMESPACE}',r.namespace) loop_xpath,
       r.xml_data_type,
       r.xml_data_size,
       r.xml_data_scale,
       r.xml_data_format,
       'O' xml_source
FROM   vl_conf_xmlcolmapoverride r)
SELECT x.table_name,
       x.column_name, 
       x.xml_path, 
       x.loop_xpath, 
       CASE 
         WHEN (x.xml_data_type = 'decimal') THEN 'Number'
         WHEN (x.xml_data_type = 'nvarchar') THEN 'String'
         WHEN (x.xml_data_type IN ('integer','int')) THEN 'Integer'
         WHEN (x.xml_data_type = 'uniqueidentifier') THEN 'String'
         WHEN (x.xml_data_type = 'datetime' AND x.xml_data_format = 'yyyy-MM-dd') THEN 'Date'
         WHEN (x.xml_data_type = 'datetime' AND x.xml_data_format &lt;&gt; 'yyyy-MM-dd') THEN 'Timestamp'
         ELSE x.xml_data_type
       END xml_data_type,
       x.xml_data_size, 
       x.xml_data_scale,
       x.xml_data_format,
       'Node' element_type,
       'Value of' result_type,
       CASE 
         WHEN x.column_name IN ('REFRESH_ID','SOURCE_SYSTEM','LOADDATE') THEN
           x.xml_path
         WHEN x.column_name LIKE 'VLSTG%_ID' THEN
           x.xml_path
         WHEN (x.table_name IN ('VL_STG_POLICY','VL_STG_POLICYVERSION','VL_STG_POLICYUNIT','VL_STG_POLICYTERM',
                                'VL_STG_POLICY_WC','VL_STG_LOCATION_WC','VL_STG_CLASSIFICATION_WC','VL_STG_COVERAGE_WC', 
                                'VL_STG_FORM_WC','VL_STG_POLICY_BOP','VL_STG_LOCATION_BOP','VL_STG_CLASSIFICATION_BOP',
                                'VL_STG_COVERAGE_BOP','VL_STG_FORM_BOP','VL_STG_BUILDING_BOP','VL_STG_COVERAGE_PW',
                                'VL_STG_POLICY_PW','VL_STG_POLICY_CU','VL_STG_RISK_CU','VL_STG_COVERAGE_CU') AND 
               x.column_name IN ('TRANSACTION_UNIQUEID')) THEN 
           x.xml_path
         ELSE
           x.column_name
       END field_name,
       x.xml_source
FROM   x 
WHERE  x.xml_path IS NOT NULL
AND    x.table_name = '${V_TABLE_NAME}'
ORDER  BY  x.column_name</sql>
