        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var v_valid_xml;
var v_loop_xpath;
var v_subloop_xpath;
var v_search_result;
var v_namespace;
var v_looppath_array;

v_namespace = "xmlns:"+replace(NAMESPACE,":","");
v_loop_xpath = replace(LOOP_XPATH,"//","");

if (indexOf(v_loop_xpath,"/")>0) {
  v_subloop_xpath = substr(v_loop_xpath, 0,indexOf(v_loop_xpath,"/"));
} else {
  v_subloop_xpath = v_loop_xpath;
}

// Check if the loop_xpath exist in the input XML file.
v_search_result = indexOf(v_file_content, v_loop_xpath);

v_looppath_array = v_subloop_xpath.split("|");

if (v_search_result == -1) {
  // If the loop xpath is a search by tableType, then check if the namespace is valid.
  if (indexOf(v_loop_xpath,"tableType=") > -1) {
     
    if (indexOf(v_file_content,v_namespace) > -1) {
       v_valid_xml = "Y";
    } else {
       v_valid_xml = "N";
    }
  } else {
    // Check if the loop xpath has multiple path
     if (indexOf(v_loop_xpath,"|") > -1) {
        for (i=0;i&lt;v_looppath_array.length;i++) {
            if (indexOf(v_file_content,v_looppath_array[i]) > -1) {
                v_valid_xml = "Y";
            }
        }
     } else {
         if (indexOf(v_file_content,v_subloop_xpath) > -1) {
            v_valid_xml = "Y";
         } else {
            v_valid_xml = "N";
         }
      } 
  }
} else {
  v_valid_xml = "Y";
}  </jsScript_script>
