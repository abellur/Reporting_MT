    <sql>SELECT refresh_id, 
       webservice_name, 
       output_xml_path, 
       table_name, 
       loop_xpath, 
       namespace, 
       valid_xpath
FROM   vl_conf_chkxmllooppath 
WHERE  valid_xpath = 'Y'
AND    refresh_id = ${vl_refresh_id}
AND    table_name &lt;&gt; 'VL_STG_PREMIUMDETAIL'
UNION 
SELECT ${vl_refresh_id} refresh_id,
       o.webservice_name, 
       o.output_xml_path,
       t.table_name, 
       replace(xm.loop_xpath,'{NAMESPACE}',xm.namespace) as loop_xpath, 
       xm.namespace, 
       'Y' valid_xpath
FROM   vl_xmloutput o,
       vl_conf_wstablemapping m, 
       vl_conf_tabledefinition t, 
       vl_conf_columndefinition c,
       vl_conf_xmlcolmapping  xm
WHERE  m.tabledefinition_id = t.tabledefinition_id
AND    m.webservice_name = o.webservice_name
AND    t.tabledefinition_id = c.tabledefinition_id
AND    xm.columndefinition_id = c.columndefinition_id
AND    o.refresh_id = ${vl_refresh_id}
AND    t.table_name = '${V_INPUT_TABLE_NAME}'
AND    t.table_name = 'VL_STG_POLICYVERSIONLIST'
ORDER  BY table_name</sql>
