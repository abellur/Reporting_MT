    <connection>Velocity_Staging</connection>
    <sql>SELECT v1.xml_filename,
       v1.xml_path,
       max(CASE 
             WHEN v1.xml_data_type_description = 'START_ELEMENT' THEN 
               xml_data_name
           END) element_name,
       max(CASE
             WHEN v1.xml_data_name||v1.xml_data_type_description = 'dataTypeATTRIBUTE' THEN
               xml_data_value
           END) data_type,
       max(CASE
             WHEN v1.xml_data_name||v1.xml_data_type_description = 'sizeATTRIBUTE' THEN
               xml_data_value
           END) data_size,
       max(CASE 
             WHEN v1.xml_data_name||v1.xml_data_type_description = 'scaleATTRIBUTE' THEN 
               xml_data_value
           END) data_scale
FROM   vl_conf_xmlsource v1
WHERE  v1.xml_data_type_description NOT IN ('END_ELEMENT')
AND    v1.xml_data_name NOT IN ('changed','isWip','title')
GROUP  BY v1.xml_filename, v1.xml_path
ORDER  BY 1, 2
</sql>
