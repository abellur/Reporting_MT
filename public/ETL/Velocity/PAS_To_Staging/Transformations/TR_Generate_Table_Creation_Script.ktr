      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var updated_column_name;
var temp_column_name;
var updated_data_type;

// Translate long column name to shorter name since Oracle prior to 12.2 does
// not support column name longer than 30 characters.

if (COLUMN_NAME.length &lt;= 30) {
    updated_column_name = COLUMN_NAME;
} 
else {
    temp_column_name = replace(COLUMN_NAME,"WorkersComp", "wc", "Package","pkg", "Federal","fd",
                                  "Liability","lib","IncrLimits","inclim","Minimum","min",
                                  "Premium","prem","Admiralty","adm","Coverage","cvg",
                                  "PostARD","pard","TotalSubject","totsub","BalanceTo","balto",
                                  "Policy","pol","Employers","emp","Indicator","ind","Total","tot",
                                  "Modified","mod","Liabililty","lib","State","st","Expense","exp",
                                  "Standard","std","Adjustment","Adj","Program","Pgm","Deductible","Ded",
                                  "Accident","Acc","Amount","Amt","Aggregate","Agg","Request","Req",
                                  "Additional","Add","Noncompliance","Noncom","Management","Mgt",
                                  "Cerfified","Cert","Construction","Constr","Contracting","Cotr",
                                  "Disease","Dis","Designated","Desig","Medical","Med","Provider","Pvd",
                                  "Class","Cls","Code","cd","Certificate","Cert","Change","Chg",
                                  "Attendance","Att","Average","Avg","Percentage","Pct","Workplace","Wplc",
                                  "Territory","Terr","Transition","Trans","Employee","Empe",
                                  "VolunteerFirefighters","VolFF","Volunteer","Vol","Ambulance","Amb",
                                  "Outside","Out","Population","Pop","Total","Tot","Manual","Man",
                                  "DrugFree","DF","WindstormRoofWall","WRW","Asphalt","AT","Metal","MT",
                                  "MortarAdhesive","MA","SinglePlyMembrane","SP","WoodShingles","WS",
                                  "Plywood","PW","SprinklerLeakageEQ","SLEQ","TemporarilyAwayFromPremises","TAFP",
                                  "ElectricalCableInstallationContractors","ECIC","FastFood","FF",
                                  "LandscapingContractors","LC","Limited","Lt","PrintersErrorsOmissions","PEO",
                                  "CarpentryInterior","CI","ConcreteMasonryContractors","CMC","ConvenienceStore","CS",
                                  "RestaurantRisk","RR");
    
    if (temp_column_name.length &lt;= 30) {
        updated_column_name = temp_column_name;
    } else {
        updated_column_name = temp_column_name.slice(0,26) + temp_column_name.slice(-4);
    }     
}

updated_column_name = upper(replace(updated_column_name,"-",""));

// Some columns have same name, but data type is different, for example:
// Same column in one coverage, the field's data type is integer, 
// while in another coverage, the field's data type is int. We want to convert
// them to same data type.

if (DATA_TYPE == "integer" || DATA_TYPE == "int") {
    updated_data_type = "int"
} else {
    if ( DATA_TYPE == "decimal") {
       updated_data_type = "number"
    } else {
       updated_data_type = DATA_TYPE
    }
}
</jsScript_script>
