    <connection>4SBI_Staging</connection>
    <sql>WITH c AS 
       (SELECT v1.xml_path,
               max(DECODE(v1.xml_data_type_description,'START_ELEMENT',xml_data_name)) column_name,
               max(DECODE(v1.xml_data_name||v1.xml_data_type_description,'dataTypeATTRIBUTE',xml_data_value)) data_type,
               max(DECODE(v1.xml_data_name||v1.xml_data_type_description,'sizeATTRIBUTE',xml_data_value)) data_size,
               max(DECODE(v1.xml_data_name||v1.xml_data_type_description,'scaleATTRIBUTE',xml_data_value)) data_scale
        FROM   vl_conf_xmlsource v1
        WHERE  v1.xml_filename = '${v_filename}'
        AND    v1.xml_data_type_description NOT IN ('END_ELEMENT')
        AND    v1.xml_data_name NOT IN ('changed','isWip','title')
        AND    v1.xml_parent_path IN 
               (SELECT v.xml_path
               FROM   vl_conf_xmlsource v
               WHERE  v.xml_filename = '${v_filename}'
               AND    v.xml_data_name = 'tableType'
               AND    v.xml_data_value = '${v_tabletype}'
               )
        GROUP  BY v1.xml_path)
SELECT DISTINCT c.column_name, c.data_type, c.data_size, c.data_scale
        FROM  c
        WHERE c.data_type IS NOT NULL
order  by column_name</sql>
