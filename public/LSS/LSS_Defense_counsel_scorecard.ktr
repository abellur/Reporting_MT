  <step>
    <name>gtfrom_defense_counsel</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>MAG LSS</connection>
    <sql>

SELECT distinct  
  dbo_ALL_COMPANIES11.COMPANY_NAME as law_firm_name,
  dbo_EMPLOYEES18.LAST_NAME +', '+dbo_EMPLOYEES18.FIRST_NAME as Attorney_name,
  dbo.ATTORNEY_RATING_COMPOSITE.COMPOSITE_RATING,
  dbo.MATTERS.MATTER_ID,
  dbo.MATTERS.MATTER_NAME,
  dbo_TIMEKEEPER_RATES2.TIMEKEEPER_CATEGORY_CD
FROM
  dbo.ALL_COMPANIES  dbo_ALL_COMPANIES11 RIGHT OUTER JOIN dbo.MATTER_FIRMS ON (dbo.MATTER_FIRMS.FIRM_ID=dbo_ALL_COMPANIES11.COMPANY_ID)
   RIGHT OUTER JOIN dbo.MATTERS ON (dbo.MATTERS.MATTER_ID=dbo.MATTER_FIRMS.MATTER_ID)
   LEFT OUTER JOIN dbo.INVOICES ON (dbo.INVOICES.MATTER_ID=dbo.MATTERS.MATTER_ID)
   INNER JOIN dbo.INVOICE_DETAILS ON (dbo.INVOICE_DETAILS.INVOICE_ID=dbo.INVOICES.INVOICE_ID)
   LEFT OUTER JOIN dbo.EMPLOYEES  dbo_EMPLOYEES22 ON (dbo.INVOICE_DETAILS.EMPLOYEE_ID=dbo_EMPLOYEES22.EMPLOYEE_ID)
   LEFT OUTER JOIN dbo.TIMEKEEPER_RATES  dbo_TIMEKEEPER_RATES2 ON (dbo_EMPLOYEES22.EMPLOYEE_ID=dbo_TIMEKEEPER_RATES2.EMPLOYEE_ID)
   INNER JOIN dbo.ALL_COMPANIES ON (dbo.ALL_COMPANIES.COMPANY_ID=dbo.INVOICES.DIVISION_ID)
   LEFT OUTER JOIN dbo.DIRECTORY_ADDRESSES ON (dbo.ALL_COMPANIES.STATE_ID=dbo.DIRECTORY_ADDRESSES.STATE_ID)
  RIGHT OUTER JOIN dbo.DIRECTORY_ADDRESS_ACTIVITY_V ON (dbo.DIRECTORY_ADDRESS_ACTIVITY_V.directory_addresses_id=dbo.DIRECTORY_ADDRESSES.DIRECTORY_ADDRESSES_ID)
   LEFT OUTER JOIN dbo.EMPLOYEES  dbo_EMPLOYEES23 ON (dbo.DIRECTORY_ADDRESS_ACTIVITY_V.created_by=dbo_EMPLOYEES23.EMPLOYEE_ID)
   LEFT OUTER JOIN dbo.ATTORNEY_RATING_COMPOSITE ON (dbo.MATTERS.MATTER_ID=dbo.ATTORNEY_RATING_COMPOSITE.MATTER_ID)
   LEFT OUTER JOIN dbo.EMPLOYEES  dbo_EMPLOYEES18 ON (dbo.ATTORNEY_RATING_COMPOSITE.ATTORNEY_ID=dbo_EMPLOYEES18.EMPLOYEE_ID)
  
WHERE
( dbo_ALL_COMPANIES11.COMPANY_TYPE_CD = 'L' or dbo_ALL_COMPANIES11.COMPANY_TYPE_CD IS NULL  )
  AND  ( dbo_TIMEKEEPER_RATES2.HISTORY_SEQ_NBR = '0' AND dbo_TIMEKEEPER_RATES2.APPROVAL_IND = 'Y'  )
 

 and
 dbo.ATTORNEY_RATING_COMPOSITE.COMPOSITE_RATING is not null</sql>
