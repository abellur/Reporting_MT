    <sql>TRUNCATE TABLE STG_MINI_PM_POLNO_XREF;

COMMIT;

INSERT
INTO STG_MINI_PM_POLNO_XREF
  (
    XREF_ID,
    MINI_UNIQID,
    MINI_POL_NO,
    MINI_EFFDTID,
    MINI_EXPDTID,
    NEW_POL_NO,
    NEW_UNIQID,
    NEW_EFFDTID,
    NEW_EXPDTID
  )
 

SELECT ROWNUM AS XREF_ID, TAB.*
FROM
(

			SELECT 
			L.POL_UNIQUEID AS MINI_UNIQID, 
			L.POL_POLICYNUMBER AS MINI_POL_NO,
			L.POLICYEFFECTIVEDATE_ID AS MINI_EFFDTID,
			L.POLICYEXPIRATIONDATE_ID AS MINI_EXPDTID, 
			L.POLICY_NO AS NEW_POL_NO,
			CEFF.POL_UNIQUEID AS NEW_UNIQID,
			CEFF.POLICYEFFECTIVEDATE_ID AS NEW_EFFDTID,
			CEFF.POLICYEXPIRATIONDATE_ID AS NEW_EXPDTID
			FROM
					(
					 SELECT 
					   HPXREF.POL_UNIQUEID, 
					   HPXREF.POL_POLICYNUMBER,
					   LEFF.POLICYEFFECTIVEDATE_ID,
					   LEFF.POLICYEXPIRATIONDATE_ID, 
					   HPXREF.POLICY_NO
					 FROM
						 (
						  SELECT DISTINCT

							P1.POL_UNIQUEID,
						   P1.POL_POLICYNUMBER,
							CASE
						   WHEN LENGTH(P1.POL_POLICYNUMBER) = 8
						   OR LENGTH(P1.POL_POLICYNUMBER)   = 7
						   THEN P1.POL_POLICYNUMBER
						   WHEN LENGTH(P1.POL_POLICYNUMBER) > 7
						   AND HFL.POL_NO                  IS NOT NULL
						   AND SUBSTR(P1.POL_POLICYNUMBER, 1, 3) LIKE '%F%'
						   THEN SUBSTR(P1.POL_POLICYNUMBER, 5, 7)
							 || 'A'
						   WHEN LENGTH(P1.POL_POLICYNUMBER) > 7
						   AND RegExp_Like(SUBSTR(P1.POL_POLICYNUMBER, 1, 3), '([[:alpha:]]$)')
						   AND SUBSTR(P1.POL_POLICYNUMBER, 5, 7) IN
							 (SELECT DISTINCT DIM_POLICY.POL_POLICYNUMBER
							 FROM DIM_POLICY,
							FACT_POLICY
							 WHERE DIM_POLICY.POLICY_ID = FACT_POLICY.POLICY_ID
							 )
						   THEN SUBSTR(P1.POL_POLICYNUMBER, 5, 7)
						   WHEN LENGTH(P1.POL_POLICYNUMBER) > 7
						   AND RegExp_Like(SUBSTR(P1.POL_POLICYNUMBER, 1, 3), '([[:alpha:]]$)')
						   AND SUBSTR(P1.POL_POLICYNUMBER, 5, 7) NOT IN
							 (SELECT DISTINCT DIM_POLICY.POL_POLICYNUMBER
							 FROM DIM_POLICY,
							FACT_POLICY
							 WHERE DIM_POLICY.POLICY_ID = FACT_POLICY.POLICY_ID
							 )
						   THEN P2.POL_POLICYNUMBER
						   WHEN LENGTH(P1.POL_POLICYNUMBER) = 12
						   AND RegExp_Like(SUBSTR(P1.POL_POLICYNUMBER, 1, 3), '([[:digit:]]$)')
						   THEN SUBSTR(P1.POL_POLICYNUMBER, 1, 7)
						   WHEN LENGTH(P1.POL_POLICYNUMBER) = 10
						   AND RegExp_Like(SUBSTR(P1.POL_POLICYNUMBER, 1, 3), '([[:digit:]]$)')
						   THEN P1.POL_POLICYNUMBER
							END AS POLICY_NO
							
						  FROM DIM_POLICY P1
						  LEFT JOIN DIM_POLICY P2
						  ON P2.POL_POLICYNUMBER LIKE (SUBSTR(P1.POL_POLICYNUMBER, 5, 7)
							|| '%')
						  LEFT JOIN
							(SELECT SUBSTR(P.POL_POLICYNUMBER, 1, 7) POL_NO,
						   COUNT(DISTINCT P.POL_POLICYNUMBER) AS HOW_MANY
							FROM DIM_POLICY P,
						   FACT_POLICY F
							WHERE P.POLICY_ID = F.POLICY_ID
							GROUP BY SUBSTR(P.POL_POLICYNUMBER, 1, 7)
							HAVING COUNT(DISTINCT P.POL_POLICYNUMBER) > 1
							) HFL
						  ON HFL.POL_NO LIKE (SUBSTR(P1.POL_POLICYNUMBER, 5, 7)
							|| '%')
						  INNER JOIN FACT_POLICYCOVERAGE F
						  ON F.POLICY_ID = P1.POLICY_ID
						  OR F.POLICY_ID = P2.POLICY_ID
						  WHERE P1.POL_POLICYNUMBER NOT LIKE 'RLP%'
						  AND (
							(P1.POL_POLICYNUMBER LIKE 'HPL%')
							  OR (P1.POL_POLICYNUMBER LIKE 'HLP%')
							  OR (P1.POL_POLICYNUMBER LIKE 'HUL%')
							  OR (P1.POL_POLICYNUMBER LIKE 'HXL%')
							  OR (P1.POL_POLICYNUMBER LIKE 'HFL%')
							  OR (P1.POL_POLICYNUMBER LIKE 'HUF%')
							  OR (P1.POL_POLICYNUMBER LIKE 'HXF%')
							  )
							AND (P1.POL_POLICYNUMBER NOT LIKE 'H%BS')
							AND (P1.POL_POLICYNUMBER NOT LIKE 'HCS%')
						 )HPXREF
					 INNER JOIN 
						 (
						  SELECT DISTINCT DP.POL_UNIQUEID, DP.POL_POLICYNUMBER,  FP.POLICYEFFECTIVEDATE_ID,
							FP.POLICYEXPIRATIONDATE_ID
						  FROM FACT_POLICYSNAPSHOT FP
						  INNER JOIN DIM_POLICY DP
						  ON DP.POLICY_ID = FP.POLICY_ID

						  UNION

						  SELECT DISTINCT DP.POL_UNIQUEID, DP.POL_POLICYNUMBER, FC.POLICYEFFECTIVEDATE_ID,
							FC.POLICYEXPIRATIONDATE_ID
						  FROM FACT_CLAIM FC
						  INNER JOIN DIM_POLICY DP
						  ON DP.POLICY_ID = FC.POLICY_ID
						  AND (
							(DP.POL_POLICYNUMBER LIKE 'HPL%')
							  OR (DP.POL_POLICYNUMBER LIKE 'HLP%')
							  OR (DP.POL_POLICYNUMBER LIKE 'HUL%')
							  OR (DP.POL_POLICYNUMBER LIKE 'HXL%')
							  OR (DP.POL_POLICYNUMBER LIKE 'HFL%')
							  OR (DP.POL_POLICYNUMBER LIKE 'HUF%')
							  OR (DP.POL_POLICYNUMBER LIKE 'HXF%')
							  )
							AND (DP.POL_POLICYNUMBER NOT LIKE 'H%BS')
							AND (DP.POL_POLICYNUMBER NOT LIKE 'HCS%')
						  ORDER BY POL_UNIQUEID,  POL_POLICYNUMBER,POLICYEFFECTIVEDATE_ID
						 )LEFF
					 ON HPXREF.POL_UNIQUEID = LEFF.POL_UNIQUEID
					 AND HPXREF.POL_POLICYNUMBER = LEFF.POL_POLICYNUMBER
					)L
			INNER JOIN 
					
					(
					 SELECT DISTINCT DP.POL_UNIQUEID, DP.POL_POLICYNUMBER,  FP.POLICYEFFECTIVEDATE_ID,
					   FP.POLICYEXPIRATIONDATE_ID
					 FROM FACT_POLICYSNAPSHOT FP
					 INNER JOIN DIM_POLICY DP
					 ON DP.POLICY_ID = FP.POLICY_ID

					 UNION

					 SELECT DISTINCT DP.POL_UNIQUEID, DP.POL_POLICYNUMBER, FC.POLICYEFFECTIVEDATE_ID,
					   FC.POLICYEXPIRATIONDATE_ID
					 FROM FACT_CLAIM FC
					 INNER JOIN DIM_POLICY DP
					 ON DP.POLICY_ID = FC.POLICY_ID
					 AND (
					   (DP.POL_POLICYNUMBER LIKE 'HPL%')
						 OR (DP.POL_POLICYNUMBER LIKE 'HLP%')
						 OR (DP.POL_POLICYNUMBER LIKE 'HUL%')
						 OR (DP.POL_POLICYNUMBER LIKE 'HXL%')
						 OR (DP.POL_POLICYNUMBER LIKE 'HFL%')
						 OR (DP.POL_POLICYNUMBER LIKE 'HUF%')
						 OR (DP.POL_POLICYNUMBER LIKE 'HXF%')
						 )
					   AND (DP.POL_POLICYNUMBER NOT LIKE 'H%BS')
					   AND (DP.POL_POLICYNUMBER NOT LIKE 'HCS%')
					 ORDER BY POL_UNIQUEID,  POL_POLICYNUMBER,POLICYEFFECTIVEDATE_ID
					)CEFF
			ON L.POLICY_NO = CEFF.POL_POLICYNUMBER
			AND L.POLICYEFFECTIVEDATE_ID = CEFF.POLICYEFFECTIVEDATE_ID
			--AND L.POLICYEXPIRATIONDATE_ID = CEFF.POLICYEXPIRATIONDATE_ID
			ORDER BY NEW_POL_NO, NEW_EFFDTID, NEW_EXPDTID
)
TAB
;
COMMIT;</sql>
