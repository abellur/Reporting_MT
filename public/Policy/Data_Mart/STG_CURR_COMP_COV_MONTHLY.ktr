    <connection>S_OLAP_PROD_DRA_17</connection>
    <sql>SELECT DISTINCT FPCOV.POLICY_ID AS CURR_POL_ID,
  FPCOV.FIRSTINSURED_ID AS CURR_FI_ID,
  FPCOV.COVEREDRISK_ID AS CURR_COVRSK_ID,
  FPCOV.COVERAGE_ID AS CURR_COVG_ID,
  FPCOV.MONTH_ID AS CURR_MONTH_ID,
  FPCOV.POLICYEFFECTIVEDATE_ID AS CURR_POLEFFDT_ID,
  FPCOV.POLICYEXPIRATIONDATE_ID AS CURR_POLEXPDT_ID,
  DC.COV_TYPE AS CURR_COV_TYP,
  DC.COV_NAME AS CURR_COV_NAME,
  DIM_LIMIT.COV_LIMIT1 AS CGL_LIMIT,
  DIM_LIMIT.COV_LIMIT2 AS CGL_AGG
 FROM FACT_POLICYCOVERAGE FPCOV
INNER JOIN DIM_COVERAGE DC
ON FPCOV.COVERAGE_ID = DC.COVERAGE_ID
INNER JOIN DIM_COMPONENT DCOM
ON FPCOV.COMPONENT_ID = DCOM.COMPONENT_ID
LEFT JOIN VDIM_COVEREDRISKNAME VCR
ON VCR.COVEREDRISK_ID = FPCOV.COVEREDRISK_ID
INNER JOIN DIM_LIMIT
ON FPCOV.LIMIT_ID = DIM_LIMIT.LIMIT_ID
--INNER JOIN DIM_COVERAGEEXTENSION DE
--ON FPCOV.COVERAGEEXTENSION_ID = DE.COVERAGEEXTENSION_ID
WHERE 
 DCOM.MMCOM_COMPONENT_TYPE_CODE NOT IN (
'DEDUCTIBLE',
'DISCOUNT',
'SURCHARGE',
'ADJUSTMENT',
'SIR' )

--AND FPCOV.POLICY_ID ='117514' AND  FPCOV.MONTH_ID ='20160601' AND  FPCOV.COVERAGE_ID ='174'
AND FPCOV.LIMIT_ID &lt;&gt; '0'  --AND FPCOV.POLICY_ID ='117492'
--AND DE.MMCOV_SHARED_LIMIT_B IS NOT NULL
-- IGNORING LIMIT_ID = '0' AS COVERAGE WITHOUT THE DOLLAR VALUE

AND  FPCOV.MONTH_ID = ${MONTH_ID}
</sql>
