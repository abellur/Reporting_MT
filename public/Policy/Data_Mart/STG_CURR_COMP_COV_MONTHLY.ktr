  DC.COV_NAME AS CURR_COV_NAME,
  DCRX.MMRSK_INC_LIMIT AS CGL_LIMIT,
  DCRX.MMRSK_AGG_LIMIT AS CGL_AGG
FROM VFACT_CLAIM VFC
INNER JOIN DIM_POLICY DP
ON DP.POLICY_ID = VFC.POLICY_ID
INNER JOIN DIM_POLICYEXTENSION DPX
ON VFC.POLICYEXTENSION_ID = DPX.POLICYEXTENSION_ID
INNER JOIN DIM_COVERAGE DC
ON DC.COVERAGE_ID = VFC.COVERAGE_ID
INNER JOIN DIM_COVERAGEEXTENSION DCX
ON DCX.COVERAGEEXTENSION_ID = VFC.COVERAGEEXTENSION_ID
INNER JOIN DIM_COVEREDRISKEXTENSION DCRX
ON DCRX.COVEREDRISKEXTENSION_ID    = VFC.COVEREDRISKEXTENSION_ID
WHERE 
--VFC.COVEREDRISKEXTENSION_ID IN
