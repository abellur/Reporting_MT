    <sql>SELECT DISTINCT 
  DP.POLICY_ID     AS COMP_POL_ID,
  VFC.FIRSTINSURED_ID            AS COMP_FI_ID,
  VFC.COVEREDRISK_ID             AS COMP_COVRSK_ID,
  VFC.COVERAGE_ID                AS COMP_COVG_ID,
  VFC.MONTH_ID                   AS COMP_MONTH_ID,
  VFC.POLICYEFFECTIVEDATE_ID     AS COMP_POLEFFDT_ID,
  VFC.POLICYEXPIRATIONDATE_ID    AS COMP_POLEXPDT_ID,
  DC.COV_TYPE                    AS COMP_COV_TYP,
  DC.COV_NAME                    AS COMP_COV_NAME,
  DCRX.MMRSK_CHAR1               AS CGL_COMPONENT_TYP,
  DCX.MMCOV_DEDUCTIBLE_TYPE_DESC AS CGL_IND_EXP_BOTH,
  DD.COV_DEDUCTIBLE1     AS COMP_VALUE,
  DD.COV_DEDUCTIBLE2 AS COMP_AGG_VALUE

FROM VFACT_CLAIM VFC
INNER JOIN DIM_POLICY DP
ON DP.POLICY_ID = VFC.POLICY_ID
INNER JOIN DIM_POLICYEXTENSION DPX
ON VFC.POLICYEXTENSION_ID = DPX.POLICYEXTENSION_ID
INNER JOIN DIM_COVERAGE DC
ON DC.COVERAGE_ID = VFC.COVERAGE_ID
INNER JOIN DIM_COVERAGEEXTENSION DCX
ON DCX.COVERAGEEXTENSION_ID = VFC.COVERAGEEXTENSION_ID
INNER JOIN DIM_COVEREDRISKEXTENSION DCRX
ON DCRX.COVEREDRISKEXTENSION_ID = VFC.COVEREDRISKEXTENSION_ID
INNER JOIN DIM_DEDUCTIBLE DD
ON VFC.DEDUCTIBLE_ID               = DD.DEDUCTIBLE_ID
WHERE VFC.COVEREDRISKEXTENSION_ID IN
  (SELECT DIM_COVEREDRISKEXTENSION.COVEREDRISKEXTENSION_ID
  FROM DIM_COVEREDRISKEXTENSION
  WHERE DIM_COVEREDRISKEXTENSION.MMRSK_CHAR1 IN ('Ded', 'SR')
AND 

VFC.MONTH_ID   = ${MONTH_ID}
  )
AND VFC.POLICY_ID IN
  (SELECT DIM_POLICY.POLICY_ID
  FROM DIM_POLICY
  WHERE (DIM_POLICY.POL_POLICYNUMBER LIKE 'HPL%')
  OR (DIM_POLICY.POL_POLICYNUMBER LIKE 'HLP%')
  OR (DIM_POLICY.POL_POLICYNUMBER LIKE 'HUL%')
  OR (DIM_POLICY.POL_POLICYNUMBER LIKE 'HXL%')
  )
--AND VFC.CLAIM_NO = '46600'  
ORDER BY COMP_POL_ID,
  COMP_COVRSK_ID,
  COMP_COV_TYP</sql>
