    <connection>MAGSTAGE</connection>
    <sql>SELECT DISTINCT LB.AGE,LB.AGENTNO,LB.AMENDMENT,LB.AUDITCODE,LB.BRANCH,LB.CANCELDATE,LB.CAPEXPDT,LB.CAPRETRODT,LB.CITY,LB.CLASS,LB.CMY,LB.COMPLINE,LB.COVEFFDTE,LB.COVEXPDTE,LB.COVINC,LB.CTRCDE,LB.CUSTNO,LB.DEDPER,LB.ENTERDATE,LB.ENTIDN,LB.ENTITYTYPE,LB.ENTNAM,LB.ENTNTP,LB.EXCCLAIM,LB.FINAUDIT,LB.HISTOPT,LB.IMPDED,LB.INSCNT,LB.INSTALNO,LB.INSTALTERM,LB.KINDCODE,LB.LIMITAGGER,LB.LIMITOCCUR,LB.LOB,LB.LOCATION,LB.LYRLIT,
LB.MAGYRS,LB.MANUAL,LB.MASTER0CO,LB.MODECODE,LB.MODULE,LB.MRSSEQ,LB.MRSUID,LB.NUMDRS,LB.NUMFTDRS,LB.OCCEXPDTE,LB.ORIGINCEPT,LB.PAYCODE,LB.PHNAME,LB.PODED,LB.POLEFF,LB.POLEXP,LB.POLICY0NUM,LB.POLICYCO,
LB.POLIDN,LB.POLNTP,LB.POLSTABBR,LB.POLSTATE,LB.PPDED,LB.PRCTYP,LB.PRCTYPDSC,LB.PROFITCTR,LB.PTDED,LB.RATCLS,LB.REASONAMND,LB.RENEWALCD,LB.RENEWPAY,LB.RENPOLNO,LB.RENSYMBOL,LB.RETRODTE,LB.REVIEWCD,
LB.RTCLSA,LB.RTGCTY,LB.RTGCTYNAME,LB.RTGSTABBR,LB.RTGTER,LB.SARSTATE,LB.SARSUBLINE,LB.SARUNIT,LB.SORTNAME,LB.SPCLTY,LB.SPECUSEA,LB.SPECUSEB,LB.SYMBOL,LB.TOTDSC,LB.TOTGES,LB.TOTOPL,LB.TOTPRM,LB.TOTSUR,
LB.TYPCOV,LB.TYPEACT,LB.UNITDSC,LB.UNITGES,LB.UNITOPL,LB.UNITPRM,LB.UNITSUR,LB.UWCMDT,LB.VARIATION,LB.AS_OF,LB.LOAD_DATE,LB.REGIONS,LB.NETPREM,LB.ISSUECODE,
AGNT.AGNM_MCO,AGNT.AGNM_AGCY,AGNT.SUBAGENT,AGNT.AGENCY_TYPE,
PR_EXT.PRODUCER_EXT,PR_EXT.NAME1,PR_EXT.NAME2,PR_EXT.EXT_AGENT,
CASE WHEN PR_INT.PRODUCER_LAST_NAME IS NULL THEN USR.USERNAML ELSE PR_INT.PRODUCER_LAST_NAME END AS PRODUCER_LAST_NAME,
CASE WHEN PR_INT.PRODUCER_FIRST_NAME IS NULL THEN USR.USERNAMF ELSE PR_INT.PRODUCER_FIRST_NAME END AS PRODUCER_FIRST_NAME,
CASE WHEN PR_INT.PRODUCER_INT IS NULL THEN USR.USERID ELSE PR_INT.PRODUCER_INT END AS PRODUCER_INT,
CASE WHEN PR_INT.INT_AGENT IS NULL THEN USR.USERNAME ELSE PR_INT.INT_AGENT END AS INT_AGENT

 FROM 
(
SELECT
  a.AGE
, a.AGENTNO
, a.AMENDMENT
, a.AUDITCODE
, a.BRANCH
, CASE WHEN A.CANCELDATE ='0000000' THEN '-' ELSE TO_CHAR(A.CANCELDATE+19000000) END AS CANCELDATE
, a.CAPEXPDT
, a.CAPRETRODT
, a.CITY
, a.CLASS
, a.CMY
, a.COMPLINE
, a.COVEFFDTE+19000000 AS COVEFFDTE
, a.COVEXPDTE+19000000 AS COVEXPDTE
, a.COVINC
, a.CTRCDE
, a.CUSTNO
, a.DEDPER
, a.ENTERDATE
, a.ENTIDN
, a.ENTITYTYPE
, a.ENTNAM
, a.ENTNTP
, a.EXCCLAIM
, a.FINAUDIT
, a.HISTOPT
, a.IMPDED
, a.INSCNT
, a.INSTALNO
, a.INSTALTERM
--, ISSUECODE
, a.KINDCODE
, a.LIMITAGGER
, a.LIMITOCCUR
, case when substr(A.policy0num,1,2) ='45' THEN 'RLF'
when substr(A.policy0num,1,2) ='55' THEN 'CLF' ELSE A.LOB  END AS LOB 
, a.LOCATION
, a.LYRLIT
, a.MAGYRS
, a.MANUAL
, a.MASTER0CO
, a.MODECODE
, a.MODULE
, a.MRSSEQ
, a.MRSUID
, a.NUMDRS
, a.NUMFTDRS
, a.OCCEXPDTE
, a.ORIGINCEPT+190000 AS ORIGINCEPT
, a.PAYCODE
, a.PHNAME
, a.PODED
, a.POLEFF+19000000 as POLEFF
, a.POLEXP+19000000 AS POLEXP
, a.POLICY0NUM
, a.POLICYCO
, a.POLIDN
, a.POLNTP
, a.POLSTABBR
, a.POLSTATE
, a.PPDED
, a.PRCTYP
, a.PRCTYPDSC
, a.PROFITCTR
, a.PTDED
, a.RATCLS
, a.REASONAMND
, a.RENEWALCD
, a.RENEWPAY
, a.RENPOLNO
, a.RENSYMBOL
, a.RETRODTE
, a.REVIEWCD
, a.RTCLSA
, a.RTGCTY
, a.RTGCTYNAME
, a.RTGSTABBR
, a.RTGTER
, a.SARSTATE
, a.SARSUBLINE
, a.SARUNIT
, a.SORTNAME
, a.SPCLTY
, a.SPECUSEA
, a.SPECUSEB
, case when substr(A.policy0num,1,2) ='45' THEN 'RLF'
when substr(A.policy0num,1,2) ='55' THEN 'CLF' ELSE A.SYMBOL  END AS SYMBOL  
, a.TOTDSC
, a.TOTGES
, a.TOTOPL
, a.TOTPRM
, a.TOTSUR
, a.TYPCOV
, a.TYPEACT
, a.UNITDSC
, a.UNITGES
, a.UNITOPL
, a.UNITPRM
, a.UNITSUR
, a.UWCMDT
, a.VARIATION
, AS_OF
, a.LOAD_DATE
,CASE WHEN a.POLSTABBR='NC' or a.POLSTABBR='SC' THEN 'The Carolinas' 
      WHEN a.POLSTABBR='AL' or a.POLSTABBR='AR' or a.POLSTABBR='KY' or a.POLSTABBR='TN' or a.POLSTABBR='IN' 
      THEN 'Western Region' ELSE a.POLSTABBR END AS Regions

,(a.UNITPRM+a.UNITDSC+a.UNITSUR+a.UNITOPL+a.UNITGES) as  Netprem

,case when (a.issuecode ='M' AND B.TYPE0ACT ='NB') OR A.ISSUECODE = 'N' THEN 'N'    
WHEN (a.issuecode ='M' AND B.TYPE0ACT ='RB') OR A.ISSUECODE = 'R' THEN 'R' ELSE A.ISSUECODE END AS ISSUECODE

FROM ALL_LOB_INFORCED a
LEFT OUTER JOIN Policy b on 
a.symbol = b.symbol and 
a.policy0num = b.policy0num and 
a.module = b.module and 
a.master0co = b.master0co 
AND  b.trans0stat ='V'

WHERE A.SYMBOL NOT IN ('PRF','HRF') AND  SUBSTR(A.SYMBOL,3,1) &lt;&gt; 'R'
AND SUBSTR(AS_OF,1,6) = (SELECT  TO_CHAR(LAST_DAY(SYSDATE),'YYYYMM')  FROM DUAL)


) LB

LEFT OUTER JOIN
  (SELECT
    AGNM_MCO,AGNM_AGCY,SUBAGENT,case when AGENCY_TYPE='MMIC' THEN 'DIRECT' WHEN AGENCY_TYPE='IND' THEN 'INDEPENDENT' ELSE AGENCY_TYPE END AS AGENCY_TYPE
    FROM AGENTXREF_XV
   ) AGNT ON AGNT.AGNM_MCO = LB.MASTER0CO     AND AGNT.AGNM_AGCY =LB.AGENTNO

LEFT OUTER JOIN
   (SELECT
      PRODUCER as PRODUCER_EXT,NAME1,NAME2,(NAME1||''||NAME2) AS Ext_Agent 
     FROM NAME_ADDRESS_XV
   ) PR_EXT ON PR_EXT.PRODUCER_EXT =LB.SPECUSEB

LEFT OUTER JOIN
   (SELECT
   LASNAM as Producer_Last_Name,FIRNAM as Producer_First_Name,Producer AS PRODUCER_INT,(FIRNAM||''||LASNAM) as Int_Agent
   FROM NAME_ADDRESS_XV
    ) PR_INT ON PR_INT.PRODUCER_INT= LB.SPECUSEA

LEFT OUTER JOIN
   ( SELECT TRIM(USERID) AS USERID,USERNAML,USERNAMF,(USERNAMF||''||USERNAML) AS USERNAME
  FROM USERID_XV
   ) USR ON USR.USERID=LB.SPECUSEA



</sql>
