    <sql>SELECT DISTINCT DP.POLICY_ID,
  FPC.MONTH_ID,
Null as CLAIM_ID,
   FPC.POLICYEFFECTIVEDATE_ID,
  FPC.POLICYEXPIRATIONDATE_ID,
  FPC.FIRSTINSURED_ID,
Null as CLMPART_ID1,
Null as CLOSEDDATE_ID,
Null as DATEOFLOSS_ID,
Null as DATEREPORTED_ID,
Null as CLAIMLOSSLOCATION_ID,
Null as CLAIMSTATUS_ID,
   FPC.LIMIT_ID,
  FPC.DEDUCTIBLE_ID,
Null as CLAIMTYPE_ID,
Null as CLAIMSUMMARY_ID,
Null as CLAIM_NO,
  FPC.PRODUCER_ID AS C_PRODUCER_ID,
  FPC.POLICYEXTENSION_ID,
Null as CLAIMANT_ID,
Null as ADJUSTER_ID, 
  FPC.COVERAGE_ID,
  FPC.COVERAGEEXTENSION_ID,
  FPC.COVEREDRISK_ID,
  FPC.COVEREDRISKEXTENSION_ID,
  FPC.UNDERWRITER_ID,

Null as INDEMNITY_PAID_ITD,
Null as INDEMINITY_RESERVE_ITD,
Null as EXPENSE_PAID_ITD,
Null as EXPENSE_RESERVE_ITD,
Null as TOTAL_INCURRED_ITD

FROM FACT_POLICYCOVERAGE FPC
INNER JOIN DIM_POLICY DP
ON DP.POLICY_ID = FPC.POLICY_ID
INNER JOIN DIM_COVERAGE Z
ON Z.COVERAGE_ID = FPC.COVERAGE_ID
LEFT OUTER JOIN VFACT_CLAIM VFC
ON FPC.POLICY_ID = VFC.POLICY_ID
INNER JOIN DIM_POLICYEXTENSION DPX
ON FPC.POLICYEXTENSION_ID = DPX.POLICYEXTENSION_ID
WHERE --VFC.CLAIM_ID IS NULL 
(Z.COV_TYPE like 'PLH%'
OR 
Z.COV_TYPE like 'HPL%'
OR 
Z.COV_TYPE like 'HLP%'
OR 
Z.COV_TYPE like 'CGL%'
OR 
Z.COV_TYPE like 'UMBPL%'
OR 
Z.COV_TYPE like 'UMBCGL%'
OR 
Z.COV_TYPE like 'HUL%')
AND DPX.MMPOL_POLICY_TYPE_CODE       = 'INST'
--AND DPX.MMPOL_ORGANIZATION_TYPE_CODE = 'HOSPITAL'
and fpc.coverage_id &lt;&gt; VFC.COVERAGE_ID  
--and DP.POLICY_ID ='116927'
--AND FPC.POLICY_ID ='116628'</sql>
