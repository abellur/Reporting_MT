    <notepad>
      <note>Modified columns feeding the data INDEMNITY_PAID_ITD, INDEMINITY_RESERVE_ITD, EXPENSE_PAID_ITD,EXPENSE_RESERVE_ITD
FROM 

VFC.LOSS_PD_AMT_ITD                                                                                       AS INDEMNITY_PAID_ITD,
  VFC.LOSS_RSRV_CHNG_AMT_ITD                                                                                  AS INDEMINITY_RESERVE_ITD,
  VFC.ALC_EXP_PD_AMT_ITD                                                                                      AS EXPENSE_PAID_ITD,
  VFC.ALC_EXP_RSRV_CHNG_AMT_ITD                                                                               AS EXPENSE_RESERVE_ITD,
  (VFC.LOSS_PD_AMT_ITD + VFC.LOSS_RSRV_CHNG_AMT_ITD + VFC.ALC_EXP_PD_AMT_ITD + VFC.ALC_EXP_RSRV_CHNG_AMT_ITD) AS TOTAL_INCURRED_ITD

TO

VFC.LOSS_PD_AMT_ITD                                                                                         AS INDEMNITY_PAID_ITD,
  VFC.LOSS_RSRV_CHNG_AMT_ITD                                                                                  AS INDEMINITY_RESERVE_ITD,
  VFC.ALC_EXP_PD_AMT_ITD                                                                                      AS EXPENSE_PAID_ITD,
  (VFC.ALC_EXP_RSRV_CHNG_AMT_ITD  - VFC.ALC_EXP_PD_AMT_ITD)                                               AS EXPENSE_RESERVE_ITD,
  (VFC.LOSS_PD_AMT_ITD + VFC.LOSS_RSRV_CHNG_AMT_ITD +VFC.ALC_EXP_PD_AMT_ITD+(VFC.ALC_EXP_RSRV_CHNG_AMT_ITD  - VFC.ALC_EXP_PD_AMT_ITD) ) AS TOTAL_INCURRED_ITD

