    <name>Policies with NO Claims on PL GL or UMB type coverages</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>STAGE_OLAP_PROD_DRA_17</connection>
    <sql>SELECT DISTINCT 
--DP.POL_POLICYNUMBER, 
DP.POLICY_ID,

  FPC.MONTH_ID,
Null as CLAIM_ID,
   FPC.POLICYEFFECTIVEDATE_ID,
  FPC.POLICYEXPIRATIONDATE_ID,
  FPC.FIRSTINSURED_ID,
Null as CLMPART_ID1,
Null as CLOSEDDATE_ID,
Null as DATEOFLOSS_ID,
Null as DATEREPORTED_ID,
Null as CLAIMLOSSLOCATION_ID,
Null as CLAIMSTATUS_ID,
NULL AS LIMIT_ID,
NULL AS DEDUCTIBLE_ID,
Null as CLAIMTYPE_ID,
Null as CLAIMSUMMARY_ID,
Null as CLAIM_NO,
NULL AS C_PRODUCER_ID,
  FPC.POLICYEXTENSION_ID,
Null as CLAIMANT_ID,
Null as ADJUSTER_ID, 
FPC.COVERAGE_ID AS COVERAGE_ID,
NULL AS COVERAGEEXTENSION_ID,
  FPC.COVEREDRISK_ID,
NULL AS COVEREDRISKEXTENSION_ID,
NULL AS UNDERWRITER_ID,

Null as INDEMNITY_PAID_ITD,
Null as INDEMINITY_RESERVE_ITD,
Null as EXPENSE_PAID_ITD,
Null as EXPENSE_RESERVE_ITD,
Null as TOTAL_INCURRED_ITD



FROM MAGDWR.FACT_POLICYSNAPSHOT FPC
INNER JOIN MAGDWR.DIM_POLICY DP
ON DP.POLICY_ID = FPC.POLICY_ID
INNER JOIN MAGDWR.DIM_COVERAGE Z
ON Z.COVERAGE_ID = FPC.COVERAGE_ID
AND (Z.COV_TYPE like 'PLH%' OR Z.COV_TYPE like 'UMBPL%' OR Z.COV_TYPE like 'CGL%' )
INNER JOIN MAGDWR.DIM_POLICYEXTENSION DPX
ON FPC.POLICYEXTENSION_ID = DPX.POLICYEXTENSION_ID
INNER JOIN MAGDWR.DIM_LEGALENTITY LE
ON LE.LEGALENTITY_ID = FPC.FIRSTINSURED_ID
LEFT JOIN (SELECT DISTINCT
                  B.NEW_UNIQID,  
                  B.NEW_POL_NO,
                  CP.COV_TYPE,
                  M2.LOSSRUN_MAP_CODE AS NEW_LR_MAP_CODE,
                  B.NEW_EFFDTID,
                  B.NEW_EXPDTID
                FROM
                  (SELECT DISTINCT X.MINI_UNIQID,
                    X.MINI_POL_NO,
                    CC.COV_TYPE        AS MINI_COVTYPE,
                    M.LOSSRUN_MAP_CODE AS MINI_LR_MAP_CODE,
                    X.MINI_EFFDTID,
                    X.MINI_EXPDTID,
                    X.NEW_POL_NO,
                    X.NEW_UNIQID,
                    X.NEW_EFFDTID,
                    X.NEW_EXPDTID
                  FROM STG_MINI_PM_POLNO_XREF X
                  INNER JOIN MAGDWR.DIM_POLICY PC
                  ON PC.POL_UNIQUEID = X.MINI_UNIQID
                  INNER JOIN MAGDWR.VFACT_CLAIM F
                  ON F.POLICY_ID = PC.POLICY_ID
                  INNER JOIN MAGDWR.DIM_COVERAGE CC
                  ON F.COVERAGE_ID = CC.COVERAGE_ID
                  INNER JOIN MAGDWR.MAG_COVERAGE_MAPPING M
                  ON M.COVERAGE_CODE = CC.COV_TYPE
                  --WHERE X.MINI_POL_NO LIKE '%18%'
                  ) B
                INNER JOIN MAGDWR.DIM_POLICY PP
                ON PP.POL_UNIQUEID = B.NEW_UNIQID
                INNER JOIN MAGDWR.FACT_POLICYSNAPSHOT FP
                ON FP.POLICY_ID = PP.POLICY_ID
                INNER JOIN MAGDWR.DIM_COVERAGE CP
                ON FP.COVERAGE_ID = CP.COVERAGE_ID
                AND CP.COV_TYPE  &lt;&gt; 'PRIMARY'
                AND (CP.COV_TYPE LIKE 'PLH%'
                OR CP.COV_TYPE LIKE 'UMBPL%'
                OR CP.COV_TYPE LIKE 'CGL%')
                INNER JOIN MAGDWR.MAG_COVERAGE_MAPPING M2
                ON M2.COVERAGE_CODE     = CP.COV_TYPE
                AND M2.LOSSRUN_MAP_CODE = B.MINI_LR_MAP_CODE
)NP
ON NP.NEW_UNIQID = DP.POL_UNIQUEID  
AND NP.NEW_POL_NO = DP.POL_POLICYNUMBER
