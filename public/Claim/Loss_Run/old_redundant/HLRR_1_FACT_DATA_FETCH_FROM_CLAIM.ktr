  VFC.DATEOFLOSS_ID,
  VFC.DATEREPORTED_ID,
  VFC.CLAIMLOSSLOCATION_ID,
  VFC.CLAIMSTATUS_ID,
  VFC.LIMIT_ID,
  VFC.DEDUCTIBLE_ID,
  VFC.CLAIMTYPE_ID,
  VFC.CLAIMSUMMARY_ID,
  VFC.CLAIM_NO,
