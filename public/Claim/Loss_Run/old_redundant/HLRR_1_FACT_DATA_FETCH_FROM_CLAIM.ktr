  VFC.CLAIM_ID,
  VFC.POLICYEFFECTIVEDATE_ID,
  VFC.POLICYEXPIRATIONDATE_ID,
  VFC.FIRSTINSURED_ID,
  VFC.CLMPART_ID1,
  CASE
    WHEN VFC.CLOSEDDATE_ID = 19000101
