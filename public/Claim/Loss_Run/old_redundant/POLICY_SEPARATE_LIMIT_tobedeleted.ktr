    <sql>SELECT DISTINCT CE.MMCOV_SHARED_LIMIT_B,
  CE.COV_UNIQUEID AS SEPARATE_COVERAGE_KEY,
  P.POL_POLICYNUMBER,
  C.COV_TYPE,
  FI.FINSD_FORMATTED_NAME      AS POLICY_HOLDER,
  R.COVEREDRISK_FORMATTED_NAME AS RISK,
  DE.EN_ENTITY_TYPE            AS COV_RISK_EN_TYPE,
  DC.CVRSK_TYPE                AS CVRSK_TYPE_CODE,
  CASE
    WHEN DE.EN_ENTITY_TYPE = 'Organization'
    AND DC.CVRSK_TYPE     IN ('HOSP', 'FACILITY')
    THEN 'Print Banner'
    WHEN DE.EN_ENTITY_TYPE = 'Person'
    AND L.COV_LIMIT1 > 0
    THEN 'Print Banner'
    ELSE 'Dont Print Banner'
  END AS BANNER_HELPER,
  L.COV_LIMIT1,
  L.COV_LIMIT2
FROM MAGDWR.DIM_POLICY P
INNER JOIN MAGDWR.FACT_POLICYCOVERAGE F
ON F.POLICY_ID = P.POLICY_ID
INNER JOIN MAGDWR.DIM_COVERAGE C
ON F.COVERAGE_ID = C.COVERAGE_ID
INNER JOIN MAGDWR.DIM_COVERAGEEXTENSION CE
ON F.COVERAGEEXTENSION_ID = CE.COVERAGEEXTENSION_ID
INNER JOIN MAGDWR.VDIM_FIRSTINSURED FI
ON FI.FIRSTINSURED_ID = F.FIRSTINSURED_ID
INNER JOIN MAGDWR.VDIM_COVEREDRISKNAME R
ON R.COVEREDRISK_ID = F.COVEREDRISK_ID
INNER JOIN MAGDWR.DIM_COVEREDRISK DC
ON DC.COVEREDRISK_ID = F.COVEREDRISK_ID
INNER JOIN MAGDWR.DIM_ENTITY DE
ON DC.CVRSK_NUMBER = DE.EN_CLIENT_ID
INNER JOIN MAGDWR.DIM_LIMIT L
ON F.LIMIT_ID              = L.LIMIT_ID
WHERE CE.COV_UNIQUEID NOT IN
  (SELECT DISTINCT STG_POL_SHR_GRP.SHARE_COVERAGE_KEY FROM STG_POL_SHR_GRP
  )
AND (C.COV_TYPE LIKE 'CGL%'
OR C.COV_TYPE LIKE 'PL%'
OR C.COV_TYPE LIKE 'UMB%')
ORDER BY RISK,
  C.COV_TYPE</sql>
